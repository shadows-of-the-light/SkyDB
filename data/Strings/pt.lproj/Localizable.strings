"ap24_fq06_chat_02" = "Sim, a Ninny acha que o mar é grande demais.";
"ap24_fq06_chat_03" = "Olá.";
"ap24_fq06_chat_04" = "Como está a criança invisível?";
"ap24_fq06_chat_05" = "Só sobrou a cabeça.";
