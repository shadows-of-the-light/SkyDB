"ap24_fq04_story_01" = "Naquela noite, Mãe Mumin fez um vestido rosa para Ninny e o deixou no sótão leste.";
"ap24_fq04_story_02" = "Ninny desceu para se juntar à família Mumin para o chá da manhã.";
"ap24_fq05_chat_01" = "Ninny aparecia só até o pescoço, restando apenas o rosto.";
"ap24_fq05_chat_02" = "Mumintroll ficou animado. Decidiu ensinar a Ninny todos os jogos que ele conhecia.";
"ap24_fq05_chat_03" = "Sobe aí, Ninny!";
"ap24_fq05_chat_04" = "Desculpe, não posso...";
"ap24_fq05_chat_05" = "Corre, corre, não consegue?";
"ap24_fq05_chat_06" = "Desculpa...";
"ap24_fq05_chat_07" = "Não consegue nem pular?";
"ap24_fq05_chat_08" = "Dava para ver que ela jogava apenas por educação, e não porque estava se divertindo.";
"ap24_fq05_chat_09" = "Eu sinto muito...";
"ap24_fq05_chat_10" = "Você acha que alguém gosta disso?";
"ap24_fq05_chat_11" = "Você não tem vida dentro de você?";
"ap24_fq05_chat_12" = "Quer um soco no nariz?";
"ap24_fq05_chat_13" = "Ela não sabe jogar...";
"ap24_fq05_chat_14" = "Ela não consegue ficar brava.";
"ap24_fq05_chat_15" = "Esse é o problema dela.";
"ap24_fq05_chat_16" = "Você nunca vai ter seu próprio rosto até aprender a brigar!";
"ap24_fq05_chat_17" = "Sim, claro.";
