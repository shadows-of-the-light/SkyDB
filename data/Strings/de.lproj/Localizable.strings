"ap21_quest01_narration_01" = "Hier ist die Geschichte des Neunfarbigen Hirsches …";
"ap21_quest01_narration_02" = "… ein gütiges Wesen aus vergangener Zeit.";
"ap21_quest01_narration_03" = "Eines Tages verirrte sich ein Kind";
"ap21_quest01_narration_04" = "in einem düsteren Tal.";
"ap21_quest01_narration_05" = "Der Neunfarbige Hirsch rettete das Kind";
"ap21_quest01_narration_06" = "und sagte: \"Komm und besuche mein schönes Heim.“";
"ap21_quest01_narration_07" = "So rettete der Neunfarbige Hirsch";
"ap21_quest01_narration_08" = "das Kind vor einem Monster.";
"ap21_quest01_narration_09" = "Das Kind hatte Angst";
"ap21_quest01_narration_10" = "und wollte fliehen.";
"ap21_quest02_gameplay_01" = "Finde die Krabbe, die sich im Sand versteckt.";
"ap21_quest02_gameplay_02" = "Finde die Qualle, die sich auf dem Grund des Sees versteckt.";
"ap21_quest02_gameplay_03" = "Entdecke den Manta von der Düne aus.";
"ap21_quest02_hint1" = "Finde die Krabbe und die Qualle am See.";
"ap21_quest02_hint2" = "Führe die Qualle zum <1>Hirsch</1>.";
"ap21_quest02_hint3" = "Führe die Krabbe zum <1>Hirsch</1>.";
"ap21_quest02_hint4" = "Besuche den <1>Hirsch</1> um Hilfe.";
"ap21_quest02_narration_01" = "Eines Tages kam ein Monster zur Sicheloase.";
"ap21_quest02_narration_02" = "Die Tiere versteckten sich voller Furcht.";
"ap21_quest02_narration_03" = "Der Neunfarbige Hirsch suchte nach den Tieren …";
"ap21_quest02_narration_04" = "… doch sie waren alle fort.";
"ap21_quest02_narration_05" = "Der Neunfarbige Hirsch wusste, was zu tun war.";
"ap21_quest02_narration_06" = "Gütig wacht der Neunfarbige Hirsch";
"ap21_quest02_narration_07" = "über alle Lebewesen.";
"ap21_quest03_hint0" = "Nimm die Einladung des <2>Neunfarbigen Hirsches</2> an und besucht gemeinsam die Oase.";
"ap21_quest03_narration_01" = "Eines Tages sehnte sich der Neunfarbige Hirsch nach einem Gefährten.";
"ap21_quest03_narration_02" = "Mit dem Kind feierte der Hirsch die Schönheit des Lebens.";
"ap21_quest03_narration_03" = "Ein Manta erschien – er wurde von einem Dorfbewohner gejagt!";
"ap21_quest03_narration_04" = "Aber der Manta war zu schnell …";
"ap21_quest03_narration_05" = "… und der Dorfbewohner fiel in den See.";
"ap21_quest03_narration_06" = "Der Neunfarbige Hirsch wusste, was zu tun war.";
"ap21_quest03_narration_07" = "Der Hirsch opferte etwas Magie …";
"ap21_quest03_narration_08" = "… und erweckte den Dorfbewohner wieder zum Leben.";
"ap21_quest03_narration_09" = "Aus Dankbarkeit bot der Dorfbewohner an, dem Hirsch ewig zu dienen.";
"ap21_quest03_narration_10" = "Der Hirsch bat nur darum, ihre Begegnung geheim zu halten …";
"ap21_quest03_narration_11" = "… doch der Dorfbewohner wusste, er würde belohnt werden …";
"ap21_quest03_narration_12" = "… denn die Prinzessin begehrte den Hirsch.";
"ap21_quest03_narration_13" = "So erzählte der Dorfbewohner dem König, was er gesehen hatte.";
"ap21_quest03_narration_14" = "Der König zog aus, um den Hirsch zu jagen.";
"ap21_quest03_narration_15" = "Und so wurde der Neunfarbige Hirsch verraten.";
"ap21_quest04_hint1" = "Hilf dem Hirsch, die im See gefangenen Kreaturen zu retten.";
"ap21_quest04_narration_01" = "Sieh, der Verrat des Neunfarbigen Hirsches.";
"ap21_quest04_narration_02" = "Auf Geheiß des königlichen Jägers";
"ap21_quest04_narration_03" = "verdarb der Dorfbewohner das Wasser.";
"ap21_quest04_narration_04" = "So wurde der edlen Kreatur eine Falle gestellt.";
"ap21_quest04_narration_05" = "Das Gift des Jägers machte den Dorfbewohner krank.";
"ap21_quest04_narration_06" = "Der Hirsch wollte helfen, doch wurde er schwächer.";
"ap21_quest04_narration_07" = "Der Hirsch opferte den letzten Lebensfunken, um den Dorfbewohner zu retten.";
"ap21_quest04_narration_08" = "Der König war über den Erfolg des Jägers erfreut …";
"ap21_quest04_narration_09" = "… doch der Dorfbewohner war von Schuld geplagt.";
"ap21_quest04_narration_10" = "Und so wurde der Neunfarbige Hirsch vom König gefangen.";
"ap21_quest05_hint0" = "Entferne die Dunkelpflanzen, um die Plattform anzuheben.";
"ap21_quest05_hint1" = "Kehre zum Hirsch zurück.";
"ap21_quest05_narration_01" = "Sieh, die Erlösung des Neunfarbigen Hirsches.";
"ap21_quest05_narration_02" = "Schatten verschlangen das Reich.";
"ap21_quest05_narration_03" = "Geschwächt konnte der Hirsch sich der Dunkelheit nicht erwehren.";
"ap21_quest05_narration_04" = "Der Dorfbewohner wollte alles richten.";
"ap21_quest05_narration_05" = "Der Neunfarbige Hirsch wusste, was zu tun war.";
"ap21_quest05_narration_06" = "Beschämt schwor der König, fortan alle Kreaturen zu schützen.";
"ap21_quest05_narration_07" = "So kehrten Frieden und Wohlstand in das Königreich zurück …";
"ap21_quest05_narration_08" = "… und der Hirsch entschwand in die Wildnis …";
"ap21_quest05_narration_09" = "… wo er bis zum Ende seiner Tage lebte.";
