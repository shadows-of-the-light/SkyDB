"ap24_credits_01" = "Vielen Dank fürs Spielen ...";
"ap24_credits_02" = "Die Saison des Mumins";
"ap24_credits_03" = "Basierend auf „Das unsichtbare Kind“";
"ap24_credits_04" = "von Tove Jansson";
"ap24_credits_05" = "Mit ausgewählten Szenen aus den Büchern";
"ap24_credits_06" = "„Komet im Mumintal“";
"ap24_credits_07" = "„Finn Familie Mumintroll“";
"ap24_credits_08" = "„Geschichten aus dem Mumintal“";
"ap24_fq01_chat_01" = "Ich bringe dich zu deinem neuen Zuhause.";
"ap24_fq01_chat_02" = "Komm schon!";
"ap24_fq01_chat_03" = "Wir sind fast da.";
"ap24_fq01_chat_04" = "Na, komm schon!";
"ap24_fq01_chat_05" = "Wen bringst du?";
"ap24_fq01_chat_06" = "Ihr Name ist Ninny.";
"ap24_fq01_chat_07" = "Sie fürchtete sich vor einer Dame, die sich um sie kümmerte, ohne sie wirklich zu mögen ...";
"ap24_fq01_chat_08" = "... und sie wurde unsichtbar.";
"ap24_fq01_chat_09" = "Ich habe diese Dame getroffen, und sie war schrecklich – auf eine eisig-ironische Art.";
"ap24_fq01_chat_10" = "Wie unangenehm!";
"ap24_fq01_chat_11" = "Wenn Leute oft Angst haben, werden sie manchmal unsichtbar.";
"ap24_fq01_chat_12" = "Gib dem Kind einen Stuhl.";
"ap24_fq01_chat_13" = "Dies ist deine neue Familie!";
"ap24_fq01_story_01" = "An einem regnerischen Abend führte eine freundliche Person ein unsichtbares Mädchen durch das Mumintal.";
"ap24_fq01_story_02" = "Der Name der Figur lautet Too-Ticky.";
"ap24_fq01_story_03" = "Vielleicht konnten ihre Freunde dabei helfen, das Mädchen wieder sichtbar zu machen.";
"ap24_fq02_chat_01" = "Schlaf so lange, wie du möchtest. Am Morgen gibt es jederzeit Tee für dich.";
"ap24_fq02_story_01" = "Die silberne Glocke klingelte hinter Muminmama auf dem Weg nach oben.";
"ap24_fq02_story_02" = "Schlaf gut, Ninny.";
"ap24_fq03_chat_01" = "Mama! Sie hat Pfoten! Man kann ihre Pfoten sehen!";
"ap24_fq03_chat_02" = "Großartig! Und noch besser wird es, wenn sie eines Tages ihre Schnauze zeigt.";
"ap24_fq03_chat_03" = "Sie haben Äpfel gepflückt. Es war ein wunderschöner Herbstmorgen.";
"ap24_fq03_chat_04" = "Gute Arbeit, Ninny!";
"ap24_fq03_chat_05" = "Oh, das war das Glas, das wir immer den Hummeln gegeben haben.";
"ap24_fq03_chat_06" = "Jetzt müssen wir es nicht runter zum Feld tragen.";
"ap24_fq03_chat_07" = "Ich kann ihre Beine sehen!";
"ap24_fq04_chat_01" = "Ich danke euch allen so sehr.";
"ap24_fq04_chat_02" = "Wir freuen uns, heute mehr von dir zu sehen!";
"ap24_fq04_chat_03" = "Schön, dass du angefangen hast, zu sprechen. Kennst du gute Spiele?";
"ap24_fq04_chat_04" = "Nein ...";
"ap24_fq04_chat_05" = "... aber ich habe von Spielen gehört!";
"ap24_fq04_story_01" = "In dieser Nacht hat Muminmama Ninny ein rosafarbenes Kleid gemacht und es vor sie in den östlichen Raum des Dachbodens gelegt.";
"ap24_fq04_story_02" = "Ninny ging nach unten, um mit der Muminfamilie ihren morgendlichen Tee zu trinken.";
"ap24_fq05_chat_01" = "Ninny war bis zu ihrem Hals sichtbar und alles, was übrig war, war ihr Gesicht.";
"ap24_fq05_chat_02" = "Mumintroll war erfreut. Er entschloss sich, Ninny alle Spiele beizubringen, die er kannte.";
"ap24_fq05_chat_03" = "Klettere hoch, Ninny!";
"ap24_fq05_chat_04" = "Tut mir leid, das kann ich nicht ...";
"ap24_fq05_chat_05" = "Lauf, lauf! Kannst du das nicht?";
"ap24_fq05_chat_06" = "Entschuldige ...";
"ap24_fq05_chat_07" = "Kannst du nicht einmal springen?";
"ap24_fq05_chat_08" = "Es war allen klar, dass sie nur aus Höflichkeit spielte und nicht, um Spaß zu haben.";
"ap24_fq05_chat_09" = "Es tut mir so leid ...";
"ap24_fq05_chat_10" = "Glaubst du, irgendjemandem gefällt das?";
"ap24_fq05_chat_11" = "Wo ist deine Lebensfreude?";
"ap24_fq05_chat_12" = "Willst du einen Schlag auf die Nase?";
"ap24_fq05_chat_13" = "Sie kann nicht spielen ...";
"ap24_fq05_chat_14" = "Sie kann nicht wütend werden.";
"ap24_fq05_chat_15" = "Das ist es, was nicht mit ihr stimmt.";
"ap24_fq05_chat_16" = "Du wirst nie ein eigenes Gesicht haben, bis du gelernt hast, wie man kämpft!";
"ap24_fq05_chat_17" = "Ja, natürlich.";
"ap24_fq06_chat_01" = "Was ist mit Ninny los?";
"ap24_fq06_chat_01b" = "Hat sie Angst?";
"ap24_fq06_chat_02" = "Ja, Ninny glaubt, dass das Meer zu groß ist.";
"ap24_fq06_chat_03" = "Hallo.";
"ap24_fq06_chat_04" = "Wie geht es dem unsichtbaren Kind?";
"ap24_fq06_chat_05" = "Nur ihr Kopf ist übrig.";
"ap24_fq06_chat_06" = "Kannst du uns mit dem Boot helfen?";
"ap24_fq06_chat_07" = "Du liebe Zeit, wie kalt es aussieht.";
"ap24_fq06_chat_08" = "Seit Wochen war nichts Aufregendes passiert.";
"ap24_fq06_chat_09" = "Gute Arbeit! ";
"ap24_fq06_chat_10" = "Ich hätte es nicht besser machen können!";
"ap24_fq06_chat_11" = "Ich sehe sie, ich sehe sie!";
"ap24_fq06_chat_12" = "Wage es nicht, sie ins große entsetzliche Meer zu schubsen!";
"ap24_fq06_story_01" = "Tage vergingen und Ninny hatte noch immer kein Gesicht.";
"ap24_fq06_story_02" = "Eines Tages ging die Familie runter zum Strand, um das Boot für den Winter einzuholen.";
"ap24_fq06_story_03" = "Als das Meer vor ihnen auftauchte, hielt Ninny plötzlich an.";
"ap24_fq06_story_04" = "Muminpapa hatte eine schelmische Idee und begann, sich von hinten an Muminmama heranzuschleichen.";
"ap24_fq06_story_05" = "Er wollte sie nicht wirklich ins Wasser schubsen, sondern einfach ein wenig die Kinder unterhalten.";
"ap24_name_little" = "Kleine My";
"ap24_name_moominmamma" = "Muminmama";
"ap24_name_moominpappa" = "Muminpapa";
"ap24_name_moomintroll" = "Mumintroll";
"ap24_name_ninny" = "Ninny";
"ap24_name_sniff" = "Sniff";
"ap24_name_snufkin" = "Schnupferich";
"ap24_name_too_ticky" = "Tooticki";
"ap24_side01_chat_01" = "Es ist der richtige Abend für ein Lied.";
"ap24_side01_chat_02" = "Ein neues Lied.";
"ap24_side01_chat_03" = "Ein Teil Erwartung ...";
"ap24_side01_chat_04" = "... zwei Teile Frühlingstraurigkeit ...";
"ap24_side01_chat_05" = "Und für den Rest,";
"ap24_side01_chat_06" = "bloß die große Wonne daran, allein zu sein und es zu mögen!";
"ap24_side02_chat_01" = "Wir saßen genau so im Frühling.";
"ap24_side02_chat_02" = "Erinnerst du dich?";
"ap24_side02_chat_03" = "Wir waren von unserem Winterschlaf erwacht und es war der allererste Tag.";
"ap24_side02_chat_04" = "Alle anderen schliefen noch.";
"ap24_side02_chat_05" = "Du hast von Plänen gesprochen.";
"ap24_side02_chat_06" = "Hast du welche für dich selbst?";
"ap24_side02_chat_07" = "Ja, ich habe einen Plan.";
"ap24_side02_chat_08" = "Aber es ist ein Einsamer, weißt du.";
"ap24_side02_chat_09" = "Du denkst darüber nach, fortzugehen.";
"ap24_side02_chat_10" = "Wann gehst du?";
"ap24_side02_chat_11" = "Jetzt – sofort!";
"ap24_side02_chat_12" = "Bleibst du lange fort?";
"ap24_side02_chat_13" = "Nein, am ersten Tag des Frühlings werde ich wieder hier unter deinem Fenster stehen und pfeifen.";
"ap24_side02_chat_14" = "Ein Jahr geht so schnell vorbei!";
"ap24_side03_chat_01" = "Du bist nicht zufällig ... äh ... ein Maler?";
"ap24_side03_chat_02" = "Oder vielleicht Poet?";
"ap24_side03_chat_03" = "Ich bin alles!";
"ap24_side03_chat_04" = "Und ihr seid Entdecker, wie ich sehe.";
"ap24_side03_chat_05" = "Und was wollt ihr entdecken?";
"ap24_side03_chat_06" = "Oh, alles.";
"ap24_side03_chat_07" = "Sterne, zum Beispiel!";
"ap24_side03_chat_08" = "Sterne!";
"ap24_side03_chat_09" = "Dann muss ich mit euch kommen.";
"ap24_side03_chat_10" = "Sterne sind meine Favoriten.";
"ap24_side03_chat_11" = "Ich lege mich immer hin und sehe sie mir an, bevor ich einschlafe ...";
"ap24_side03_chat_12" = "... und frage mich, wer auf ihnen ist.";
"ap24_side04_chat_01" = "Seuftz ...";
"ap24_side04_chat_02" = "Du wirkst bedrückt.";
"ap24_side04_chat_03" = "Ich habe mein Lieblingsspielzeug weggegeben.";
"ap24_side04_chat_04" = "Ich dachte, es würde sich gut anfühlen, etwas Nettes zutun.";
"ap24_side04_chat_05" = "Ah. Du bereust es?";
"ap24_side04_chat_06" = "Ja.";
"ap24_side04_chat_07" = "Du hast das Spielzeug verloren ...";
"ap24_side04_chat_08" = "... aber vielleicht hast du mehr erhalten, als du denkst.";
"ap24_side06_chat_01" = "Ihr Geheimnis hat etwas mit Gewitter zutun!";
"ap24_side06_chat_02" = "Sie suchen immer und sehnen sich nach Gewitter ...";
"ap24_side06_chat_03" = "Was ist in mich gefahren?";
"ap24_side06_chat_04" = "Ich bin kein Hattifattener, ich bin Muminpapa ...";
"ap24_side06_chat_05" = "Was tue ich hier?";
"ap24_side06_chat_06" = "Ich gehe nach Hause! Ich verschwinde jetzt!";
"ap24_side07_story_01" = "Einmal ist Muminpapa ohne irgendeine Erklärung davongegangen.";
"ap24_side07_story_02" = "Niemand wusste mit Sicherheit, wann Muminpapa gegangen war.";
"ap24_side07_story_03" = "Noch nie in seinem Leben hatte sich Muminpapa so friedlich gefühlt.";
"ap24_side07_story_04" = "Er fand es großartig, dass er für einen Moment nichts sagen musste ...";
"ap24_side07_story_05" = "... und nichts erklären musste, sich selbst oder anderen.";
"ap24_side07_story_06" = "Er konnte einfach sitzen und in den Himmel schauen ...";
"ap24_side07_story_07" = "... und dem Gluckern des Wassers lauschen.";
"ap24_storybook_hint01" = "Möchtest du dich in einen Schmetterling verwandeln und zum <1>Mumintal</1> reisen?";
"ap24_storybook_hint02" = "Möchtest du das <1>Mumintal</1> als Schmetterling frei erkunden?";
"ap24_storybook_hint03" = "Möchtest du zum <1>Mumintal</1> reisen?";
