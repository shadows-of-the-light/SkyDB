"ap21_quest02_narration_01" = "Un jour, un monstre vint à l'Oasis du croissant.";
"ap21_quest02_narration_02" = "Tous les animaux coururent se cacher.";
"ap21_quest02_narration_03" = "Le cerf aux neuf couleurs partit à leur recherche...";
"ap21_quest02_narration_04" = "... en vain.";
"ap21_quest02_narration_05" = "Le cerf savait quoi faire.";
"ap21_quest02_narration_06" = "Plein de bonté,";
"ap21_quest02_narration_07" = "il protège toutes les créatures.";
"ap21_quest03_hint0" = "Acceptez l'invitation du <2>cerf aux neuf couleurs</2> à visiter l'oasis ensemble.";
"ap21_quest03_narration_01" = "Un jour, le cerf aux neuf couleurs rêva de compagnie.";
"ap21_quest03_narration_02" = "Avec l'enfant, il fêta la beauté du monde.";
"ap21_quest03_narration_03" = "Une raie manta apparut... traquée par un villageois !";
"ap21_quest03_narration_04" = "Mais elle était trop rapide...";
"ap21_quest03_narration_05" = "et l'homme tomba dans le lac.";
"ap21_quest03_narration_06" = "Le cerf savait quoi faire.";
"ap21_quest03_narration_07" = "Usant d'un peu de magie...";
"ap21_quest03_narration_08" = "il le ramena à la vie.";
"ap21_quest03_narration_09" = "Pour le remercier, le villageois lui proposa ses services.";
"ap21_quest03_narration_10" = "Le cerf lui demanda juste garder leur rencontre secrète...";
"ap21_quest03_narration_11" = "Mais l'homme savait qu'il pouvait en tirer quelque chose...";
"ap21_quest03_narration_12" = "car la princesse désirait l'animal.";
"ap21_quest03_narration_13" = "L'homme alla donc voir le roi pour tout lui raconter.";
"ap21_quest03_narration_14" = "Le souverain organisa une battue.";
"ap21_quest03_narration_15" = "Le cerf aux neuf couleurs avait été trahi.";
"ap21_quest04_hint1" = "Aidez le cerf à sauver les créatures piégées dans le lac.";
"ap21_quest04_narration_01" = "De la trahison du cerf aux neuf couleurs...";
"ap21_quest04_narration_02" = "À la demande du chasseur du roi,";
"ap21_quest04_narration_03" = "l'homme souilla les eaux.";
"ap21_quest04_narration_04" = "C'est ainsi qu'un piège fut tendu à la noble créature.";
"ap21_quest04_narration_05" = "Le poison du chasseur avait rendu l'homme malade.";
"ap21_quest04_narration_06" = "Le cerf voulait l'aider, mais il s'affaiblissait.";
"ap21_quest04_narration_07" = "Le cerf sacrifia ses dernières forces de vie pour sauver l'homme.";
"ap21_quest04_narration_08" = "Le souverain se réjouit du succès du chasseur...";
"ap21_quest04_narration_09" = "... mais l'homme se retrouva rongé par la culpabilité.";
"ap21_quest04_narration_10" = "Ainsi le cerf aux neuf couleurs fut-il capturé par le roi.";
"ap21_quest05_hint0" = "Détruisez les plantes sombres pour faire monter l'élévateur.";
"ap21_quest05_hint1" = "Retournez auprès du cerf.";
"ap21_quest05_narration_01" = "Du salut du cerf aux neuf couleurs...";
"ap21_quest05_narration_02" = "Les ombres engloutirent le royaume.";
"ap21_quest05_narration_03" = "Affaibli, le cerf était incapable de chasser les ténèbres.";
"ap21_quest05_narration_04" = "L'homme chercha à se racheter.";
"ap21_quest05_narration_05" = "Le cerf aux neuf couleurs savait quoi faire.";
"ap21_quest05_narration_06" = "Honteux, le roi s'engagea dès lors à protéger toutes les créatures.";
"ap21_quest05_narration_07" = "C'est ainsi que la paix et la prospérité revinrent dans le royaume...";
"ap21_quest05_narration_08" = "... et que le cerf disparut dans la nature...";
"ap21_quest05_narration_09" = "... où il vécut libre jusqu'à la fin des temps.";
