"ap11_fetch_05_meet_02" = "Comment cela fonctionne-t-il ?";
"ap11_fetch_05_meet_03" = "Tu peux l'essayer pour moi ?";
"ap11_fetch_05_outro_01" = "(tousse)";
"ap11_fetch_05_outro_02" = "Certains jours, je parfumais sa planète.";
"ap11_fetch_05_outro_03" = "Certaines nuits, je faisais semblant de flétrir.";
"ap11_fetch_05_outro_04" = "Mais il ne voyait pas mon amitié,";
"ap11_fetch_05_outro_05" = "ou la joie dans mon âme.";
"ap11_fetch_05_outro_06" = "Le saura-t-il un jour ?";
