"ap21_quest01_narration_03" = "Un jour, un enfant s'égara";
"ap21_quest01_narration_04" = "dans une vallée de ténèbres.";
"ap21_quest01_narration_05" = "Le cerf aux neuf couleurs le secourut";
"ap21_quest01_narration_06" = "et lui dit : \"Viens voir ma merveilleuse maison.\"";
"ap21_quest01_narration_07" = "Ainsi, le cerf aux neuf couleurs";
"ap21_quest01_narration_08" = "sauva l'enfant des griffes d'un monstre.";
"ap21_quest01_narration_09" = "L'enfant avait peur,";
"ap21_quest01_narration_10" = "il tenta de s'enfuir.";
"ap21_quest02_gameplay_01" = "Trouvez le crabe qui se cache dans le sable.";
"ap21_quest02_gameplay_02" = "Trouvez la méduse au fond du lac.";
