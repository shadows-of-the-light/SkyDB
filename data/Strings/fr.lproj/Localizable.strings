"ap11_fetch_01_end_quote" = "Il était une fois un petit prince qui habitait une planète à peine plus grande que lui, et qui avait besoin d’un ami…";
"ap11_fetch_01_meet_01" = "J'aime ta cape.";
"ap11_fetch_01_meet_02" = "Elle est belle.";
"ap11_fetch_01_meet_03" = "Je veux te montrer quelque chose.";
"ap11_fetch_01_outro_01" = "Dans des jours comme celui-ci,";
"ap11_fetch_01_outro_02" = "Je pense affectueusement...";
"ap11_fetch_01_outro_03" = "À mon ami qui explore les cieux.";
"ap11_fetch_01_outro_04" = "La gentillesse qu'il a partagée en s'occupant de moi.";
"ap11_fetch_01_outro_05" = "J'attends donc son retour...";
"ap11_fetch_01_play_a_01" = "Ferme les yeux !";
"ap11_fetch_01_play_a_02" = "Compte jusqu'à cinq.";
"ap11_fetch_01_play_b_01" = "Je ne suis pas très doué, n'est-ce pas ?";
"ap11_fetch_01_play_b_02" = "Laisse-moi réessayer.";
"ap11_fetch_01_play_c_01" = "Jouons encore une fois.";
"ap11_fetch_01_play_hint_01" = "Trouvez le Petit Prince";
"ap11_fetch_01_star_01" = "Ce n'est pas loin !";
"ap11_fetch_01_star_02" = "Tu les entends ?";
"ap11_fetch_02_burn_01" = "Je me demande s'ils ressemblent à des baobabs.";
"ap11_fetch_02_burn_02" = "Des baobabs avec des épines.";
"ap11_fetch_02_butterfly_01" = "Il n'y a pas de rose ici.";
"ap11_fetch_02_butterfly_02" = "Que des papillons. Quelle grâce !";
"ap11_fetch_02_butterfly_03" = "Sur ma planète, nous avons sauvé deux ou trois chenilles pour qu'elles deviennent des papillons.";
"ap11_fetch_02_butterfly_04" = "Je n'en ai jamais vu autant !";
"ap11_fetch_02_butterfly_05" = "Envolons-nous !";
"ap11_fetch_02_butterfly_06" = "Cherchons là-haut !";
"ap11_fetch_02_bye_01" = "C'était amusant !";
"ap11_fetch_02_bye_02" = "Personne n'est jamais satisfait là où il est…";
"ap11_fetch_02_bye_03" = "Seuls les enfants savent ce qu'ils cherchent…";
"ap11_fetch_02_bye_04" = "J'ai des amis à découvrir, et beaucoup de choses à comprendre.";
"ap11_fetch_02_carry_01" = "Je suis content que tu sois ici.";
