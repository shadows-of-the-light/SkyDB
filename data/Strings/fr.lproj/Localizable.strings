"ap24_credits_01" = "Merci d'avoir joué…";
"ap24_credits_02" = "Saison des Moomins";
"ap24_credits_03" = "Inspiré de « L’enfant invisible »";
"ap24_credits_04" = "de Tove Jansson";
"ap24_credits_05" = "Avec une sélection de scènes tirées des livres";
"ap24_credits_06" = "« Une comète au Pays des Moomins »";
"ap24_credits_07" = "« Moomintroll Finn »";
"ap24_credits_08" = "et « Histoires de la Vallée des Moomins »";
"ap24_fq01_chat_01" = "Je te conduis à ton nouveau chez-toi.";
"ap24_fq01_chat_02" = "Allez, suis-moi !";
"ap24_fq01_chat_03" = "Nous y sommes presque.";
"ap24_fq01_chat_04" = "Allez, suis-moi !";
"ap24_fq01_chat_05" = "Qui nous amènes-tu ?";
"ap24_fq01_chat_06" = "Elle s’appelle Ninny.";
"ap24_fq01_chat_07" = "La femme qui s’occupait d’elle sans vraiment l’aimer lui faisait peur...";
"ap24_fq01_chat_08" = "... c’est comme ça qu’elle est devenue invisible.";
"ap24_fq01_chat_09" = "J’ai rencontré cette dame et crois-moi... elle était horrible. Sarcastique.";
"ap24_fq01_chat_10" = "Quelle horreur !";
"ap24_fq01_chat_11" = "Il arrive que certaines personnes deviennent invisibles quand elles vivent dans la peur.";
"ap24_fq01_chat_12" = "Propose une chaise à cette petite.";
"ap24_fq01_chat_13" = "Voici ta nouvelle famille !";
"ap24_fq01_story_01" = "Par une soirée pluvieuse, une bonne âme guidait une enfant invisible à travers la Vallée des Moomins.";
"ap24_fq01_story_02" = "La personne s’appelait Too-Ticky.";
"ap24_fq01_story_03" = "Ses amis pourraient peut-être aider la jeune fille.";
"ap24_fq02_chat_01" = "Et n’hésite pas à faire la grasse matinée. Une tasse de thé t’attendra demain matin, quelle que soit l’heure.";
"ap24_fq02_story_01" = "La cloche d’argent sonna longtemps après Moominmamma, à l’étage.";
"ap24_fq02_story_02" = "Allez, passe une bonne nuit, Ninny.";
"ap24_fq03_chat_01" = "Mamma ! Elle a des pattes ! Tu vois ses pattes !";
"ap24_fq03_chat_02" = "Magnifique ! Ce sera encore mieux quand elle nous montrera son museau, un jour.";
"ap24_fq03_chat_03" = "On ramassait des pommes. Cette matinée d’automne était radieuse.";
"ap24_fq03_chat_04" = "Beau travail, Ninny !";
"ap24_fq03_chat_05" = "Oh, c’est le pot pour les bourdons.";
"ap24_fq03_chat_06" = "Plus besoin de le porter jusqu’au champ.";
"ap24_fq03_chat_07" = "Je vois ses jambes !";
"ap24_fq04_chat_01" = "Merci à tous, vraiment.";
"ap24_fq04_chat_02" = "Ça nous fait plaisir de te voir un peu plus !";
"ap24_fq04_chat_03" = "Et c’est bien que tu aies décidé de parler. Tu connais des jeux ?";
"ap24_fq04_chat_04" = "Non...";
"ap24_fq04_chat_05" = "... mais je sais ce que c’est !";
"ap24_fq04_story_01" = "Cette nuit-là, Moominmamma confectionna une robe rose qu’elle disposa à l’attention de Ninny dans la mansarde est.";
"ap24_fq04_story_02" = "Ninny retrouva la famille Moomin au rez-de-chaussée pour le thé du matin.";
"ap24_fq05_chat_01" = "Ninny était visible jusqu’à son cou et il ne restait que son visage.";
"ap24_fq05_chat_02" = "Moomintroll était ravi. I décida d’apprendre à Ninny tous les jeux qu’il connaissait.";
"ap24_fq05_chat_03" = "Monte par ici, Ninny !";
"ap24_fq05_chat_04" = "Je suis désolée, c’est impossible…";
"ap24_fq05_chat_05" = "Allez, cours donc !";
"ap24_fq05_chat_06" = "Je regrette…";
"ap24_fq05_chat_07" = "Tu ne sais pas sauter, c’est ça ?";
"ap24_fq05_chat_08" = "Tout le monde voyait bien qu’elle jouait par politesse, mais pas par plaisir.";
"ap24_fq05_chat_09" = "Je suis vraiment désolée…";
"ap24_fq05_chat_10" = "Tu crois que c’est sympa pour les autres ?";
"ap24_fq05_chat_11" = "Tu n’as plus d’énergie, ou quoi ?";
"ap24_fq05_chat_12" = "Tu veux un coup sur le nez ?";
"ap24_fq05_chat_13" = "Elle est incapable de jouer…";
"ap24_fq05_chat_14" = "Elle est incapable de s’énerver.";
"ap24_fq05_chat_15" = "C’est quoi, son problème ?";
"ap24_fq05_chat_16" = "Si tu veux ton propre visage, tu dois apprendre à te battre !";
"ap24_fq05_chat_17" = "Oui, bien sûr.";
"ap24_fq06_chat_01" = "Qu’est-ce qui arrive à Ninny ?";
"ap24_fq06_chat_01b" = "Elle a peur ?";
"ap24_fq06_chat_02" = "Oui, Ninny pense que la mer est trop grande.";
"ap24_fq06_chat_03" = "Bonjour.";
"ap24_fq06_chat_04" = "Comment va l’enfant invisible ?";
"ap24_fq06_chat_05" = "Il ne reste que sa tête.";
"ap24_fq06_chat_06" = "On peut avoir un coup de main pour le bateau ?";
"ap24_fq06_chat_07" = "Mon Dieu, il a l’air de faire si froid…";
"ap24_fq06_chat_08" = "Rien d’intéressant n’était arrivé depuis des semaines.";
"ap24_fq06_chat_09" = "Bon travail ! ";
"ap24_fq06_chat_10" = "J’aurais pu faire mieux moi-même !";
"ap24_fq06_chat_11" = "Je la vois, je la vois !";
"ap24_fq06_chat_12" = "Surtout ne la pousse pas dans cette grande mer horrible !";
"ap24_fq06_story_01" = "Les jours passèrent et Ninny n’avait toujours pas de visage.";
"ap24_fq06_story_02" = "Un jour, la famille se rendit à la plage pour préparer le bateau avant l’hiver.";
"ap24_fq06_story_03" = "Lorsqu’elle aperçut la mer, Ninny s’arrêta.";
"ap24_fq06_story_04" = "Moominpappa eut l’idée de faire une farce à Moominmamma et s’approcha d’elle furtivement.";
"ap24_fq06_story_05" = "Il voulait juste amuser les enfants, il n’avait aucune intention de la pousser dans la mer.";
"ap24_name_little" = "Little My";
