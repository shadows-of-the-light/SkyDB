"mischief_sam_chat_8" = "C'est quoi, ce bruit ?!";
"mischief_sam_chat_9" = "Qu'est-ce qu'on fait maintenant ?";
"mischief_sam_clone_chat_1" = "A... Allô. Ce truc est allumé ? Clone 42 au rapport !";
"mischief_sam_clone_chat_2" = "Bienvenue dans le bassin des idées perdues.";
"mischief_sam_clone_chat_3" = "Elles étaient vraiment terribles, il vaut mieux les laisser ici";
