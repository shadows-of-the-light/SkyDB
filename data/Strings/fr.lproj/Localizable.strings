"ap24_side01_chat_01" = "La soirée idéale pour un peu de musique.";
"ap24_side01_chat_02" = "Une nouvelle chanson.";
"ap24_side01_chat_03" = "Une pincée d’espoir...";
"ap24_side01_chat_04" = "... deux de mélancolie printanière.";
"ap24_side01_chat_05" = "Et pour le reste,";
"ap24_side01_chat_06" = "rien que le plaisir d’un peu de solitude !";
"ap24_side02_chat_01" = "On était comme ça, au printemps.";
"ap24_side02_chat_02" = "Tu te souviens ?";
"ap24_side02_chat_03" = "On venait de se réveiller de notre hibernation, c’était le tout premier jour.";
"ap24_side02_chat_04" = "Les autres dormaient encore.";
"ap24_side02_chat_05" = "Tu m’as parlé de tes plans.";
"ap24_side02_chat_06" = "Tu en as, toi ?";
"ap24_side02_chat_07" = "Oui, j’ai un plan.";
"ap24_side02_chat_08" = "Mais il est très solitaire, tu sais.";
"ap24_side02_chat_09" = "Tu veux partir, c’est ça ?";
"ap24_side02_chat_10" = "Quand ?";
"ap24_side02_chat_11" = "Maintenant... tout de suite !";
"ap24_side02_chat_12" = "Et tu pars pour longtemps ?";
"ap24_side02_chat_13" = "Non, je serai de retour le premier jour du printemps pour siffler sous ta fenêtre.";
"ap24_side02_chat_14" = "Un an, ça passe si vite !";
"ap24_side03_chat_01" = "Dis-moi... tu ne serais pas peintre, par hasard ?";
"ap24_side03_chat_02" = "Ou poète ?";
"ap24_side03_chat_03" = "Je suis tout à la fois !";
"ap24_side03_chat_04" = "Et vous, vous explorez à ce que je vois.";
"ap24_side03_chat_05" = "Et qu’espérez-vous découvrir ?";
"ap24_side03_chat_06" = "Oh, tout.";
"ap24_side03_chat_07" = "Les étoiles, par exemple !";
"ap24_side03_chat_08" = "Les étoiles !";
"ap24_side03_chat_09" = "Alors, il faut que je vous accompagne.";
"ap24_side03_chat_10" = "J’adore les étoiles.";
"ap24_side03_chat_11" = "Tous les soirs, avant de m’endormir, je m’allonge pour les regarder...";
"ap24_side03_chat_12" = "et je me demande qui habite là-haut.";
"ap24_side04_chat_01" = "Pfff...";
"ap24_side04_chat_02" = "Ça n’a pas l’air d’aller.";
"ap24_side04_chat_03" = "J’ai donné mon jouet préféré.";
"ap24_side04_chat_04" = "Je me disais que ça devait être agréable de faire plaisir à quelqu’un.";
"ap24_side04_chat_05" = "Ah. Et tu regrettes ?";
"ap24_side04_chat_06" = "Ouais.";
"ap24_side04_chat_07" = "Tu as perdu ton jouet...";
"ap24_side04_chat_08" = "mais tu as peut-être gagné autre chose sans le savoir.";
"ap24_side06_chat_01" = "Leur secret doit partir avec les orages !";
"ap24_side06_chat_02" = "C’est après les nuages qu’ils courent, c’est ça qu’ils cherchent...";
"ap24_side06_chat_03" = "Qu’est-ce qui m’arrive ?";
"ap24_side06_chat_04" = "Je ne suis pas un Hatifnatte, je suis Moominpappa...";
"ap24_side06_chat_05" = "Qu’est-ce que je fais ici ?";
"ap24_side06_chat_06" = "Je rentre chez moi ! Je pars tout de suite !";
"ap24_side07_story_01" = "Une fois, Moominpappa a disparu sans dire un mot.";
"ap24_side07_story_02" = "Personne ne savait vraiment où il était parti.";
"ap24_side07_story_03" = "Jamais Moominpappa ne s’était senti aussi bien.";
"ap24_side07_story_04" = "Il trouvait fabuleux de n’avoir rien à dire...";
"ap24_side07_story_05" = "rien à expliquer à lui-même ou aux autres.";
"ap24_side07_story_06" = "Assis, il pouvait simplement admirer l’horizon...";
"ap24_side07_story_07" = "en écoutant l’eau couler.";
