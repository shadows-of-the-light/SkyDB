"mischief_sam_hint_chat_3" = "Mon esprit est tellement embrouillé ces derniers temps...";
"mischief_sam_riddle_1" = "Je jure avoir vu Nat s'enfuir avec une pile de vêtements de Reika...";
"mischief_sam_riddle_2" = "Tu as vu le crabe de Yoshi ? Il vient de filer avec une boîte sur sa tête.";
"mischief_sam_riddle_3" = "Est-ce qu'un balai vient de passer ?";
"mischief_sam_riddle_4" = "Je parie que Yoshi a essayé d'affronter le Dragon sombre et a perdu. Quelqu'un devrait aller le voir...";
"mischief_sam_riddle_5" = "Bloub bloub...";
"mischief_sam_riddle_6" = "Pspspspsps...";
"mischief_samantha_officechat_1" = "Bonjour !";
"mischief_samantha_officechat_2" = "Tu te demandes ce que je fais ici ? Eh bien... après le tapage que nous avons fait l'année dernière...";
"mischief_samantha_officechat_3" = "J'ai été banni vers l'équipe de nettoyage.";
"mischief_samantha_officechat_4" = "Ne dis rien à la direction, je suis en fait le Clone 13.";
"mischief_samantha_officechat_5" = "Au fait, tu as vu le chat d'Atlas ?";
"mischief_samantha_officechat_6" = "Je lui ai fait peur l'autre jour et je ne l'ai pas revu depuis.";
"mischief_samantha_officechat_7" = "Je commence à m'inquiéter....";
"mischief_skidmore_chat_1" = "Oh non, Samantha ! La direction ne va pas apprécier...";
"mischief_skidmore_chat_10" = "Il faut que je sorte d’ici !";
"mischief_skidmore_chat_11" = "Je vois une ouverture là-bas, est-ce qu’elle va jusqu’au bout ?";
"mischief_skidmore_chat_12" = "Où est-ce qu’ils sont tous passés ? Qu’est-ce qu’il se passe ici ?";
"mischief_skidmore_chat_13" = "Des farces. Que des farces, partout...";
"mischief_skidmore_chat_14" = "Nat ! Yoshi ! C’est donc par-là que vous êtes allés !";
"mischief_skidmore_chat_15" = "On doit vous sortir de là tous les deux...";
"mischief_skidmore_chat_16" = "Samanthaaaaa !!!";
"mischief_skidmore_chat_17" = "Je ne sais pas. J’étais ici quand je me suis réveillé.";
"mischief_skidmore_chat_18" = "Il faut qu’on aide ces deux-là. Ils ont pas l’air en grande forme.";
"mischief_skidmore_chat_2" = "Tu vois, c'est pour ça qu’on ne peut pas avoir de belles choses...";
"mischief_skidmore_chat_3" = "Donne-lui du travail ! Appuie sur ce bouton ! Fais chauffer ce chaudron !";
"mischief_skidmore_chat_4" = "Ces vauriens ! Où sont-ils passés ?";
"mischief_skidmore_chat_5" = "Oh non, Samantha ! Pas encore....";
"mischief_skidmore_chat_6" = "La direction ne va pas apprécier...";
"mischief_skidmore_chat_7" = "On ne peut pas avoir de jolies choses, n'est-ce pas ?";
"mischief_skidmore_chat_8" = "On a besoin de renforts ! Va chercher Nat et Yoshi !";
"mischief_skidmore_chat_9" = "Avant que tu ne partes, tu peux nous aider à allumer le chaudron ?";
"mischief_skidmore_riddle_1" = "Yoshi a mentionné quelque chose à propos d'un casse-croûte. Va voir si tu peux l'aider.";
"mischief_skidmore_riddle_2" = "Tu as entendu quelqu’un pleurer à l'instant ?";
"mischief_skidmore_riddle_3" = "Ne divos pas à la direction que nous organisons notre propre fête !";
"mischief_skidmore_riddle_4" = "Tu as peur du noir ? Moi, oui !";
"mischief_skidmore_riddle_5" = "Est-ce que cet enfant du ciel vient de rétrécir devant mes yeux ?!";
"mischief_skidmore_riddle_6" = "Des crabes avec des chapeaux... qu’est-ce qu’ils vont imaginer après... du plancton ?";
"mischief_skidmore_riddle_7" = "La fête est officiellement autorisée cette année. C'est promis !";
"mischief_skidmore_riddle_8" = "Des crabes avec des chapeaux... qu'est-ce qu'ils vont imaginer après... un dessous de verre en plancton ?";
"mischief_skidmore_riddle_9" = "C'est quoi ce bruit de grincement qui vient d'en haut ?";
"mischief_yoshi_chat_1" = "Dieu merci, tu m'as trouvé !";
"mischief_yoshi_chat_10" = "Haaah....";
"mischief_yoshi_chat_11" = "Je me sens bizarre...";
