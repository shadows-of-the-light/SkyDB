"ap24_credits_01" = "Gracias por jugar…";
"ap24_credits_02" = "Temporada de los Mumins";
"ap24_credits_03" = "Basada en \"La niña invisible\"";
"ap24_credits_04" = "de Tove Jansson";
"ap24_credits_05" = "Incluye escenas selectas de los libros";
"ap24_credits_06" = "\"La llegada del cometa\"";
"ap24_credits_07" = "\"El sombrero del mago\"";
"ap24_credits_08" = "y \"Cuentos de los Mumin en el Valle Mágico\"";
"ap24_fq01_chat_01" = "Estoy llevándote a tu nuevo hogar.";
"ap24_fq01_chat_02" = "¡Adelante!";
"ap24_fq01_chat_03" = "Ya casi llegamos.";
"ap24_fq01_chat_04" = "¡Pasen, pasen!";
"ap24_fq01_chat_05" = "¿A quién traes aquí?";
"ap24_fq01_chat_06" = "Se llama Ninny.";
"ap24_fq01_chat_07" = "Se asustó de una mujer que la estaba cuidando, aunque no le caía bien,";
"ap24_fq01_chat_08" = "y se volvió invisible.";
"ap24_fq01_chat_09" = "Yo la conocí, y era horrorosa. Irónica, pero con frialdad.";
"ap24_fq01_chat_10" = "¡Qué antipática!";
"ap24_fq01_chat_11" = "Si las personas se asustan muy seguido, a veces, se vuelven invisibles.";
"ap24_fq01_chat_12" = "Ofrécele una silla a la niña.";
"ap24_fq01_chat_13" = "¡Esta es tu nueva familia!";
"ap24_fq01_story_01" = "Una noche lluviosa, una bondadosa figura ayudó a un niña invisible a cruzar el Valle Mumin.";
"ap24_fq01_story_02" = "Se llamaba Too Ticky.";
"ap24_fq01_story_03" = "Tal vez sus amigos podrían ayudar al niña a dejar de ser invisible.";
"ap24_fq02_chat_01" = "Duerme todo lo que puedas. Habrá te esperándote en la mañana cuando tú quieras.";
"ap24_fq02_story_01" = "La campana plateada sonaba a cada paso que daba Mamá Mumin en el piso de arriba.";
"ap24_fq02_story_02" = "Que descanses, Ninny.";
"ap24_fq03_chat_01" = "¡Mamá! ¡Tiene patas! ¡Puedes verle las patas!";
"ap24_fq03_chat_02" = "¡Espléndido! Y aún mejor cuando te muestre el hocico.";
"ap24_fq03_chat_03" = "Juntaron manzanas. Era una bellísima mañana otoñal.";
"ap24_fq03_chat_04" = "¡Bien hecho, Ninny!";
"ap24_fq03_chat_05" = "Ah, ese era el frasco que les dábamos a los abejorros.";
"ap24_fq03_chat_06" = "Ya no hace falta llevarlo hasta el campo.";
"ap24_fq03_chat_07" = "¡Puedo verle las piernas!";
"ap24_fq04_chat_01" = "No saben cuánto les agradezco.";
"ap24_fq04_chat_02" = "¡Nos alegra que hoy haya más gente!";
"ap24_fq04_chat_03" = "Qué bueno que hayan empezado a hablar. ¿Conoces algún juego divertido?";
"ap24_fq04_chat_04" = "No…";
"ap24_fq04_chat_05" = "¡Pero sé qué son los juegos!";
"ap24_fq04_story_01" = "Esa noche, mamá Mumin le hizo un vestido rosa a Ninny y se lo dejó preparado en el ático del ala este de la casa.";
"ap24_fq04_story_02" = "Ninny bajó a tomar el desayuno con la familia Mumin.";
"ap24_fq05_chat_01" = "Ninny era visible hasta el cuello, y lo único que faltaba era su cara.";
"ap24_fq05_chat_02" = "Mumin estaba contentísimo. Decidió enseñarle a Ninny todos los juegos que conocía.";
"ap24_fq05_chat_03" = "¡Súbete, Ninny!";
"ap24_fq05_chat_04" = "Perdón, pero no puedo…";
"ap24_fq05_chat_05" = "¡Corre, corre como el viento!";
"ap24_fq05_chat_06" = "Lo siento…";
"ap24_fq05_chat_07" = "¿Por lo menos puedes saltar?";
"ap24_fq05_chat_08" = "Era evidente para todos que solo jugaba por educación y no para divertirse.";
"ap24_fq05_chat_09" = "Lo lamento mucho…";
"ap24_fq05_chat_10" = "¿Crees que alguien lo disfruta?";
"ap24_fq05_chat_11" = "¿No tienes ninguna chispa?";
"ap24_fq05_chat_12" = "¿Quieres que te dé un puñetazo en la nariz?";
"ap24_fq05_chat_13" = "No puede jugar…";
"ap24_fq05_chat_14" = "No puede enfadarse.";
"ap24_fq05_chat_15" = "Ese es su problema.";
"ap24_fq05_chat_16" = "¡Jamás tendrás una cara propia hasta que aprendas a luchar!";
"ap24_fq05_chat_17" = "Sí, desde luego.";
"ap24_fq06_chat_01" = "¿Qué bicho le picó a Ninny?";
"ap24_fq06_chat_01b" = "¿Está asustada?";
"ap24_fq06_chat_02" = "Sí, Ninny cree que el mar es demasiado grande.";
"ap24_fq06_chat_03" = "Hola.";
"ap24_fq06_chat_04" = "¿Cómo está la niña invisible?";
"ap24_fq06_chat_05" = "Solo queda su cabeza.";
"ap24_fq06_chat_06" = "¿Nos darías una mano con el bote?";
"ap24_fq06_chat_07" = "Vaya, qué frío parece.";
"ap24_fq06_chat_08" = "Hacía semanas que no sucedía nada emocionante.";
"ap24_fq06_chat_09" = "¡Buen trabajo! ";
"ap24_fq06_chat_10" = "¡Ni yo lo habría hecho mejor!";
"ap24_fq06_chat_11" = "¡La veo, la veo!";
"ap24_fq06_chat_12" = "¡Que ni se te ocurra empujarla a ese enorme mar horrendo!";
"ap24_fq06_story_01" = "Los días pasaban y a Ninny aún le faltaba la cara.";
"ap24_fq06_story_02" = "Un día, la familia se dirigió a la playa para acarrear y guardar el bote durante el invierno.";
"ap24_fq06_story_03" = "Cuando el mar apareció frente a sus ojos, Ninny paró en seco.";
"ap24_fq06_story_04" = "A Papá Mumin se le ocurrió una travesura y empezó a acercarse a Mamá Mumin por detrás.";
"ap24_fq06_story_05" = "No pretendía empujarla al agua de verdad, sino entretener un poco a los niños.";
"ap24_name_little" = "Pequeñita";
"ap24_name_moominmamma" = "Mamá Mumin";
"ap24_name_moominpappa" = "Papá Mumin";
"ap24_name_moomintroll" = "Mumin";
"ap24_name_ninny" = "Ninny";
"ap24_name_sniff" = "Sniff";
"ap24_name_snufkin" = "Snufkin";
"ap24_name_too_ticky" = "Too-ticky";
"ap24_side01_chat_01" = "Es la noche ideal para una canción.";
"ap24_side01_chat_02" = "Una canción nueva.";
"ap24_side01_chat_03" = "Una parte de expectativa…";
"ap24_side01_chat_04" = "Dos partes de tristeza primaveral.";
"ap24_side01_chat_05" = "Y, para el resto,";
"ap24_side01_chat_06" = "¡el gran placer de que te guste estar en soledad!";
"ap24_side02_chat_01" = "En la primavera, nos sentamos del mismo modo.";
"ap24_side02_chat_02" = "¿Lo recuerdas?";
"ap24_side02_chat_03" = "Era el primer día que nos despertábamos tras el sueño de invierno.";
"ap24_side02_chat_04" = "Los demás seguían durmiendo.";
"ap24_side02_chat_05" = "Mencionaste tus planes.";
"ap24_side02_chat_06" = "¿Tú tienes alguno?";
"ap24_side02_chat_07" = "Sí, tengo uno,";
"ap24_side02_chat_08" = "pero es un poco solitario.";
"ap24_side02_chat_09" = "Piensas marcharte.";
"ap24_side02_chat_10" = "¿Cuándo?";
"ap24_side02_chat_11" = "¡Ahora, de inmediato!";
"ap24_side02_chat_12" = "¿Te irás por mucho tiempo?";
"ap24_side02_chat_13" = "No, regresaré para el primer día de primavera y silbaré por debajo de tu ventana.";
"ap24_side02_chat_14" = "¡Qué rápido se pasa un año!";
"ap24_side03_chat_01" = "De casualidad te dedicas a… Eh… ¿La pintura?";
"ap24_side03_chat_02" = "¿O a la poesía?";
"ap24_side03_chat_03" = "¡Me dedico a todo!";
"ap24_side03_chat_04" = "Y, por lo que veo, lo suyo es descubrir cosas.";
"ap24_side03_chat_05" = "¿Qué piensan descubrir?";
"ap24_side03_chat_06" = "Ah, pues todo.";
"ap24_side03_chat_07" = "¡Por ejemplo, estrellas!";
"ap24_side03_chat_08" = "¡Estrellas!";
"ap24_side03_chat_09" = "Entonces, debo acompañarlos.";
"ap24_side03_chat_10" = "Adoro las estrellas.";
"ap24_side03_chat_11" = "Siempre me acuesto a verlas antes de dormir…";
"ap24_side03_chat_12" = "… y me pregunto quién vivirá en ellas.";
"ap24_side04_chat_01" = "Aah…";
"ap24_side04_chat_02" = "Pareces triste.";
"ap24_side04_chat_03" = "Es que regalé mi juguete favorito.";
"ap24_side04_chat_04" = "Me pareció que se sentiría bien hacer algo lindo.";
"ap24_side04_chat_05" = "Ah. ¿Y te arrepentiste?";
"ap24_side04_chat_06" = "Sí.";
"ap24_side04_chat_07" = "Aunque hayas perdido el juguete,";
"ap24_side04_chat_08" = "quizá hayas ganado más de lo que crees.";
"ap24_side06_chat_01" = "¡El secreto son las tormentas eléctricas!";
"ap24_side06_chat_02" = "Siempre buscan y anhelan tormentas eléctricas…";
"ap24_side06_chat_03" = "¿Qué bicho me picó?";
"ap24_side06_chat_04" = "No soy un hatifnate, sino Papá Mumin…";
"ap24_side06_chat_05" = "¿Qué hago aquí?";
"ap24_side06_chat_06" = "¡Me voy a casa! ¡Me marcho ya mismo!";
"ap24_side07_story_01" = "Una vez, papá Mumin se marchó sin dar ni una explicación.";
"ap24_side07_story_02" = "Nadie sabía a dónde se había ido.";
"ap24_side07_story_03" = "Papá Mumin jamás había estado tan tranquilo.";
"ap24_side07_story_04" = "Le parecía maravilloso no tener nada que decir…";
"ap24_side07_story_05" = "… ni explicar nada para variar, ni a sí mismo ni a los demás.";
"ap24_side07_story_06" = "Podía quedarse sentado admirando el horizonte…";
"ap24_side07_story_07" = "… y oyendo el gluglú del agua.";
