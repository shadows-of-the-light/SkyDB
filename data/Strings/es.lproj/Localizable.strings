"ap21_quest01_narration_01" = "Esta es la historia del ciervo de los nueve colores...";
"ap21_quest01_narration_02" = "... una bestia benévola de hace mucho tiempo.";
"ap21_quest01_narration_03" = "Un día, un niño se perdió";
"ap21_quest01_narration_04" = "en un valle repleto de oscuridad.";
"ap21_quest01_narration_05" = "El ciervo de los nueve colores rescató al niño";
"ap21_quest01_narration_06" = "y dijo: «Ven a visitar mi hermoso hogar».";
"ap21_quest01_narration_07" = "Así, el ciervo de los nueve colores";
"ap21_quest01_narration_08" = "salvó al niño de un monstruo.";
"ap21_quest01_narration_09" = "El niño tenía miedo";
"ap21_quest01_narration_10" = "e intentó escapar.";
"ap21_quest02_gameplay_01" = "Encuentra al cangrejo que se esconde en la arena.";
"ap21_quest02_gameplay_02" = "Encuentra a la medusa que se esconde en el fondo del lago.";
"ap21_quest02_gameplay_03" = "Ve a ver si puedes divisar a la mantarraya de la duna.";
"ap21_quest02_hint1" = "Encuentra al cangrejo y a la medusa cerca del lago.";
"ap21_quest02_hint2" = "Guía a la medusa hasta el <1>ciervo</1>.";
"ap21_quest02_hint3" = "Lleva el cangrejo al <1>ciervo</1>.";
"ap21_quest02_hint4" = "Visita al <1>ciervo</1> para obtener ayuda.";
"ap21_quest02_narration_01" = "Un día llegó un monstruo al oasis creciente.";
"ap21_quest02_narration_02" = "Todos los animales se escondieron por miedo.";
"ap21_quest02_narration_03" = "El ciervo de los nueve colores salió a buscar a los animales...";
"ap21_quest02_narration_04" = "... pero habían desaparecido.";
"ap21_quest02_narration_05" = "El ciervo de los nueve colores sabía bien lo que tenía que hacer.";
"ap21_quest02_narration_06" = "Con bondad, el ciervo de los nueve colores";
"ap21_quest02_narration_07" = "protege a todos los seres vivos.";
"ap21_quest03_hint0" = "Acepta la invitación del <2>ciervo de los nueve colores</2> para visitar juntos el oasis.";
"ap21_quest03_narration_01" = "Un día, el ciervo de los nueve colores quiso un acompañante.";
"ap21_quest03_narration_02" = "Con el niño, el ciervo celebró la belleza de la vida.";
"ap21_quest03_narration_03" = "Apareció una mantarraya... ¡que era cazada por un aldeano!";
"ap21_quest03_narration_04" = "Pero la mantarraya era demasiado rápida...";
"ap21_quest03_narration_05" = "... y el aldeano se cayó al lago.";
"ap21_quest03_narration_06" = "El ciervo de los nueve colores sabía bien lo que tenía que hacer.";
"ap21_quest03_narration_07" = "El ciervo sacrificó algo de magia...";
"ap21_quest03_narration_08" = "... y le devolvió la vida al aldeano.";
"ap21_quest03_narration_09" = "En agradecimiento, el aldeano se ofreció a servir al ciervo para siempre.";
"ap21_quest03_narration_10" = "El ciervo solo le pidió que el encuentro quede en secreto...";
"ap21_quest03_narration_11" = "... pero el aldeano sabía que podía obtener una recompensa...";
"ap21_quest03_narration_12" = "... porque la princesa quería al ciervo.";
"ap21_quest03_narration_13" = "Entonces el aldeano volvió y le dijo al rey lo que había visto.";
"ap21_quest03_narration_14" = "El rey se dispuso a cazar al ciervo.";
"ap21_quest03_narration_15" = "Así, traicionaron al ciervo de los nueve colores.";
"ap21_quest04_hint1" = "Ayuda al ciervo a salvar a las criaturas atrapadas en el lago.";
"ap21_quest04_narration_01" = "He aquí la traición del ciervo de los nueve colores.";
"ap21_quest04_narration_02" = "A pedido del cazador del rey,";
"ap21_quest04_narration_03" = "el aldeano contaminó las aguas.";
"ap21_quest04_narration_04" = "Así, la trampa quedó tendida para la noble criatura.";
"ap21_quest04_narration_05" = "El veneno del cazador enfermó al aldeano.";
"ap21_quest04_narration_06" = "El ciervo quería ayudar, pero se debilitaba cada vez más.";
"ap21_quest04_narration_07" = "El ciervo sacrificó lo último de su fuerza vital para salvar al aldeano.";
"ap21_quest04_narration_08" = "El rey quedó satisfecho ante el éxito del cazador...";
"ap21_quest04_narration_09" = "pero el aldeano se sentía agobiado por la culpa.";
"ap21_quest04_narration_10" = "De esta forma, el ciervo de nueve colores terminó en manos del rey.";
"ap21_quest05_hint0" = "Deshazte de las plantas oscuras para levantar el elevador.";
"ap21_quest05_hint1" = "Regresa con el ciervo.";
"ap21_quest05_narration_01" = "He aquí la salvación del ciervo de los nueve colores.";
"ap21_quest05_narration_02" = "Las sombras consumieron el reino.";
"ap21_quest05_narration_03" = "Debilitado, el ciervo no podía repeler la oscuridad.";
"ap21_quest05_narration_04" = "El aldeano intentó enmendar las cosas.";
"ap21_quest05_narration_05" = "El ciervo de los nueve colores sabía lo que había que hacer.";
"ap21_quest05_narration_06" = "Avergonzado, el rey juró que protegería a todas las criaturas a partir de entonces.";
"ap21_quest05_narration_07" = "Así, la paz y la prosperidad volvieron al reino...";
"ap21_quest05_narration_08" = "y el ciervo desapareció en lo salvaje...";
"ap21_quest05_narration_09" = "dónde vivió libre hasta el final de sus días.";
