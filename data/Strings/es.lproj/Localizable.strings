"ap11_fetch_01_bye_01" = "¡Te destacas en esto!";
"ap11_fetch_01_bye_02" = "Este es un reino curioso.";
"ap11_fetch_01_bye_03" = "¿Me pregunto si hay personas mayores aquí?";
"ap11_fetch_01_bye_04" = "Debo seguir explorando.";
"ap11_fetch_01_chase_01" = "¡Sígueme!";
"ap11_fetch_01_chase_02" = "¡No puedes atraparme!";
"ap11_fetch_01_chase_03" = "Vamos a jugar.";
"ap11_fetch_01_end_quote" = "Había una vez un principito que vivía en un planeta poco más grande que él, y necesitaba una amistad...";
"ap11_fetch_01_meet_01" = "Me gusta tu capa.";
"ap11_fetch_01_meet_02" = "Es muy hermosa.";
"ap11_fetch_01_meet_03" = "Hay algo que deberías ver.";
"ap11_fetch_01_outro_01" = "Es en días como este,";
"ap11_fetch_01_outro_02" = "que recuerdo con cariño...";
"ap11_fetch_01_outro_03" = "A mi amigo que explora los cielos.";
"ap11_fetch_01_outro_04" = "La bondad con la que me cuidaba.";
"ap11_fetch_01_outro_05" = "Y por eso, espero su regreso...";
"ap11_fetch_01_play_a_01" = "¡Cierra tus ojos!";
"ap11_fetch_01_play_a_02" = "Cuenta hasta cinco.";
"ap11_fetch_01_play_b_01" = "No soy muy bueno, ¿verdad?";
"ap11_fetch_01_play_b_02" = "Déjame intentarlo otra vez.";
"ap11_fetch_01_play_c_01" = "Juguemos otra vez.";
"ap11_fetch_01_play_hint_01" = "Encuentra a El principito";
"ap11_fetch_01_star_01" = "¡No queda lejos!";
"ap11_fetch_01_star_02" = "¿Las escuchas?";
"ap11_fetch_02_burn_01" = "Me pregunto si son como los baobabs...";
"ap11_fetch_02_burn_02" = "Baobabs con espinas.";
"ap11_fetch_02_butterfly_01" = "Aquí no hay rosas.";
"ap11_fetch_02_butterfly_02" = "Solo mariposas. ¡Qué elegante!";
"ap11_fetch_02_butterfly_03" = "En mi planeta, rescatamos a dos o tres orugas para que se conviertan en mariposas.";
"ap11_fetch_02_butterfly_04" = "¡Nunca había visto tantas!";
"ap11_fetch_02_butterfly_05" = "¡Volemos!";
"ap11_fetch_02_butterfly_06" = "¡Deberíamos buscar en la cima!";
"ap11_fetch_02_bye_01" = "Bueno, ¡eso fue divertido!";
"ap11_fetch_02_bye_02" = "Nadie está satisfecho dónde está...";
"ap11_fetch_02_bye_03" = "Solo los niños saben lo que están buscando...";
"ap11_fetch_02_bye_04" = "Tengo amistades por descubrir, y muchas cosas por entender.";
"ap11_fetch_02_carry_01" = "Me alegra que estés aquí.";
"ap11_fetch_02_carry_02" = "Las personas mayores se olvidan cómo llevar a caballito.";
"ap11_fetch_02_door_01" = "Explorar es fácil.";
"ap11_fetch_02_door_02" = "Encontrar un hogar no tanto.";
"ap11_fetch_02_end_quote" = "Todas las personas mayores fueron niños alguna vez... pero solo unos pocos lo recuerdan.";
"ap11_fetch_02_jelly_01" = "¿Las criaturas en esta cueva cantan?";
"ap11_fetch_02_jelly_02" = "Imagino que nos cantarían.";
"ap11_fetch_02_meet_01" = "Estoy perdido...";
"ap11_fetch_02_meet_02" = "¿Conoces alguna salida?";
"ap11_fetch_02_outro_01" = "Era solo una semilla cuando mi amigo me encontró.";
"ap11_fetch_02_outro_02" = "Por él, florecí radiante.";
"ap11_fetch_02_outro_03" = "Ahora busca nuevos planetas,";
"ap11_fetch_02_outro_04" = "pero ¿encontrará otra rosa como yo?";
"ap11_fetch_02_top_01" = "Conozco a un señor, que nunca ha olido una flor.";
"ap11_fetch_02_top_02" = "Nunca ha mirado una estrella.";
"ap11_fetch_02_top_03" = "Nunca ha querido a nadie.";
"ap11_fetch_02_top_04" = "Nunca ha hecho otra cosa que sumar y restar.";
"ap11_fetch_02_top_05" = "Y todo el día repite:";
"ap11_fetch_02_top_06" = "«¡Soy un hombre serio!»";
"ap11_fetch_02_top_07" = "Y esto lo llena de orgullo.";
"ap11_fetch_02_top_08" = "Pero no es un hombre.";
"ap11_fetch_02_top_09" = "¡Es un hongo!";
"ap11_fetch_02_whale_01" = "¿Hay alguien aquí?";
"ap11_fetch_02_whale_02" = "¡Qué criatura majestuosa!";
"ap11_fetch_02_whale_03" = "¿Se puede domesticar?";
"ap11_fetch_03_beach_01" = "¡Jamás vi tanta agua en ninguno de los planetas que he visitado!";
"ap11_fetch_03_beach_02" = "Desearía poder ver lo que hay más allá del horizonte.";
"ap11_fetch_03_beach_03" = "¿Puedes llevarme hasta la cima de esta isla?";
"ap11_fetch_03_beach_04" = "¡Vayamos a la cima!";
"ap11_fetch_03_belltower_01" = "¡Qué casa tan graciosa!";
"ap11_fetch_03_belltower_02" = "Es más grande que mi planeta entero.";
"ap11_fetch_03_belltower_03" = "Me gusta como suena con el viento.";
"ap11_fetch_03_belltower_04" = "Mira cómo está decorada con campanas.";
"ap11_fetch_03_belltower_05" = "Igual que el cielo está decorado con estrellas.";
"ap11_fetch_03_belltower_06" = "Las campanas deben ser para las criaturas.";
"ap11_fetch_03_belltower_07" = "Todos los niños saben eso.";
"ap11_fetch_03_belltower_08" = "Vayamos hasta esa estatua.";
"ap11_fetch_03_bye_01" = "¿Sabes...";
"ap11_fetch_03_bye_02" = "lo que significa ser \"domesticado\"?";
"ap11_fetch_03_bye_03" = "Un zorro me dijo que significa crear vínculos.";
"ap11_fetch_03_bye_04" = "¡Mira!";
"ap11_fetch_03_bye_05" = "Si creamos vínculos, tendremos necesidad el uno del otro.";
"ap11_fetch_03_bye_06" = "Entre los millones de niños...";
"ap11_fetch_03_bye_07" = "Para mí, tú serás único en el mundo.";
"ap11_fetch_03_bye_08" = "Para ti, seré único en el mundo.";
"ap11_fetch_03_end_quote" = "Si  uno  se  deja  domesticar,  corre  el  riesgo  de  llorar  un poco... ";
"ap11_fetch_03_geyser_01" = "¡Es un volcán!";
"ap11_fetch_03_geyser_02" = "Tengo tres volcanes, que deshollino todas las semanas.";
"ap11_fetch_03_geyser_03" = "Dos de los volcanes están en actividad, y el otro, extinto.";
"ap11_fetch_03_geyser_04" = "Nunca se sabe.";
"ap11_fetch_03_geyser_06" = "¿Ruido a cascada?";
"ap11_fetch_03_geyser_07" = "¡Busquémosla!";
"ap11_fetch_03_outro_01" = "Estaba preparada para los tigres,";
"ap11_fetch_03_outro_02" = "con mis fantásticas espinas.";
"ap11_fetch_03_outro_03" = "Pero mi amigo se volvió descuidado,";
"ap11_fetch_03_outro_04" = "mientras yo titubeaba en el viento.";
"ap11_fetch_03_outro_05" = "¿Mirará hacia atrás y me recordará?";
"ap11_fetch_03_statue_01" = "¿Qué son estas criaturas?";
"ap11_fetch_03_statue_02" = "¿Pueden domesticarse?";
"ap11_fetch_03_statue_03" = "¿Para ser una amistad de por vida?";
"ap11_fetch_03_statue_05" = "¿Qué es ese sonido?";
"ap11_fetch_03_statue_06" = "¡Ven conmigo!";
"ap11_fetch_03_tunnel_01" = "¡Sigamos explorando!";
"ap11_fetch_03_tunnel_02" = "¿A dónde lleva esto?";
"ap11_fetch_03_tunnel_03" = "¡Guau!";
"ap11_fetch_03_tunnel_04" = "¡Eso fue divertido!";
"ap11_fetch_03_waterfall_01" = "¿Escuchas eso?";
"ap11_fetch_03_waterfall_02" = "La cascada está cantando...";
"ap11_fetch_03_waterfall_03" = "El agua también es buena para el corazón...";
"ap11_fetch_03_waterfall_hint_01" = "¿De dónde viene el agua?";
"ap11_fetch_03_waterfall_hint_02" = "Vayamos a buscar el origen.";
"ap11_fetch_04_burn01_01" = "Hay semillas buenas y semillas malas.";
"ap11_fetch_04_burn02_01" = "Las semillas malas como los baobabs pueden volverse un desastre si no se les cuida a tiempo.";
"ap11_fetch_04_burn03_01" = "Si se trata de una ramita de rábano o de rosal, uno debe dejar que crezca como quiera.";
"ap11_fetch_04_burn04_01" = "Estas flores me recuerdan a mi rosa allá lejos.";
"ap11_fetch_04_burn05_01" = "Las flores son sensibles.";
"ap11_fetch_04_burn05_02" = "A veces debes protegerlas del viento.";
"ap11_fetch_04_burn06_01" = "Escuché a mi rosa cuando se quejaba, o se vanagloriaba...";
"ap11_fetch_04_burn06_02" = "o incluso a veces cuando no decía nada.";
"ap11_fetch_04_burn07_01" = "El tiempo que dedicas a regar tu rosa...";
"ap11_fetch_04_burn07_02" = "Es lo que la hace importante.";
"ap11_fetch_04_burn_start_hint" = "Elimina la oscuridad para poder cuidar las flores.";
"ap11_fetch_04_bye01_01" = "Estas rosas son hermosas.";
"ap11_fetch_04_bye01_02" = "Pero están vacías.";
"ap11_fetch_04_bye01_03" = "Mi rosa es única en todo el mundo.";
"ap11_fetch_04_bye01_04" = "Mi rosa perfumaba todo mi planeta...";
"ap11_fetch_04_bye01_05" = "Pero no bastó para complacerme.";
"ap11_fetch_04_bye02_01" = "Mi zorro me dijo una vez...";
"ap11_fetch_04_bye02_02" = "Solo con el corazón se puede ver bien.";
"ap11_fetch_04_bye02_03" = "Lo esencial es invisible a los ojos.";
"ap11_fetch_04_end_quote" = "Las estrellas son hermosas por la flor que no ves...";
"ap11_fetch_04_meet_01" = "Algo curioso se mueve por allí.";
"ap11_fetch_04_meet_02" = "¡A estas flores les falta el aire!";
"ap11_fetch_04_outro_01" = "Su risa desapareció.";
"ap11_fetch_04_outro_02" = "Mi vanidad derrotada.";
"ap11_fetch_04_outro_03" = "Sin mi amigo a mi lado,";
"ap11_fetch_04_outro_04" = "estas puestas de sol eran menos dulces.";
"ap11_fetch_04_outro_05" = "Me pregunto si se arrepentirá algún día.";
"ap11_fetch_05_bye_01" = "Gracias...";
"ap11_fetch_05_bye_02" = "Me alegro de tenerte como amistad.";
"ap11_fetch_05_end_quote" = "Uno se vuelve responsable para siempre de lo que domestica.";
"ap11_fetch_05_meet_01" = "Encontré esto en la cueva que visitamos.";
"ap11_fetch_05_meet_02" = "¿Cómo funciona?";
"ap11_fetch_05_meet_03" = "¿Te importaría probarla por mí?";
"ap11_fetch_05_outro_01" = "(tos)";
"ap11_fetch_05_outro_02" = "Algunos días, perfumaba su planeta.";
"ap11_fetch_05_outro_03" = "Algunas noches, fingía marchitarme.";
"ap11_fetch_05_outro_04" = "Pero él no veía mi amistad,";
"ap11_fetch_05_outro_05" = "ni la felicidad de mi alma.";
"ap11_fetch_05_outro_06" = "¿Alguna vez sabrá que...?";
"ap11_fetch_05_reflect_01" = "Debí juzgarla por sus actos...";
"ap11_fetch_05_reflect_02" = "y no por sus palabras.";
"ap11_fetch_05_reflect_04" = "¿Crees que soy responsable de mi rosa?";
"ap11_fetch_05_sunset_01" = "Me gustan mucho las puestas de sol.";
"ap11_fetch_05_sunset_02" = "Igual que en casa.";
"ap11_fetch_05_sunset_03" = "Un día, ¡vi la puesta de sol cuarenta y cuatro veces!";
"ap11_fetch_05_sunset_04" = "Me pregunto si mi rosa aún mira las puestas de sol. ";
"ap11_fetch_05_sunset_05" = "Me pregunto si está a salvo del viento.";
"ap11_fetch_05_sunset_07" = "¿Podrías tocar otra canción para mí?";
"ap11_fetch_06_campfire_01" = "Voy a hacerte un regalo...";
"ap11_fetch_06_campfire_02" = "Ese es mi regalo. Solo eso.";
"ap11_fetch_06_campfire_03" = "Esta noche regreso a casa.";
"ap11_fetch_06_campfire_04" = "Viviré en una de las estrellas.";
"ap11_fetch_06_campfire_05" = "En una de ellas me reiré.";
"ap11_fetch_06_campfire_06" = "Así que cuando mires hacia el cielo por la noche...";
"ap11_fetch_06_campfire_07" = "será como si todas las estrellas estuvieran riendo.";
"ap11_fetch_06_campfire_08" = "Es muy lejos y será muy difícil...";
"ap11_fetch_06_campfire_09" = "Tendré mucho más miedo.";
"ap11_fetch_06_campfire_10" = "Parecerá que estoy sufriendo.";
"ap11_fetch_06_campfire_11" = "No vengas a ver eso.";
"ap11_fetch_06_campfire_12" = "Por la noche mirarás hacia las estrellas.";
"ap11_fetch_06_campfire_13" = "Mi estrella solo será una estrella más para ti.";
"ap11_fetch_06_campfire_14" = "Y por eso amarás mirar todas las estrellas en los cielos...";
"ap11_fetch_06_campfire_15" = "Todas serán tus amigas.";
"ap11_fetch_06_end_quote" = "¿Cómo alcanzarlo? ¿Dónde encontrarlo? Es tan misteriosa, la tierra de las lágrimas.";
"ap11_fetch_06_outro_01" = "(tose)";
"ap11_fetch_06_outro_02" = "Mi tiempo se acaba...";
"ap11_fetch_06_outro_03" = "Pido sinceras disculpas.";
"ap11_fetch_06_outro_04" = "Aun así, no ha vuelto.";
"ap11_fetch_06_outro_05" = "¿Alguna vez volverá a buscarme?";
"ap11_fetch_07_end_quote" = "Mi estrella solo será una estrella más para ti. Y por eso amarás mirar todas las estrellas en los cielos. Todas serán tus amigas.";
"ap11_fetch_07_end_quote_01" = "Mi estrella solo será una estrella más para ti.";
"ap11_fetch_07_end_quote_02" = "Y por eso amarás mirar todas las estrellas en los cielos.";
"ap11_fetch_07_end_quote_03" = "Todas serán tus amigas.";
