"ap26_chase_intro" = "Tangkaplah burung biru untuk mengungkapkan sifat aslinya.";
"ap26_feather_hint" = "Sebuah <1>bulu biru</1>! Burung itu pasti berada di dekatnya...";
"ap26_fetch01_fragment_intro_00" = "Kumpulkan bulu-bulu dari burung biru untuk menambah kemeriahan perayaan!";
"ap26_fetch01_intro_00" = "“Aku tahu ada seekor burung, berwarna biru seperti safir...";
"ap26_fetch01_intro_01" = "Ikuti <1>Remaja Acak Rambut Mewah</1> ke Benteng untuk mencari <1>Burung Biru</1>?";
"ap26_fetch01_intro_02" = "... yang kicauannya terdengar di pesta yang mewah.”";
"ap26_fetch01_outro_01" = "“Seekor burung ditemukan, dan bulu-bulunya berwarna biru...";
"ap26_fetch01_outro_02" = "... tapi menghilang seperti asap lilin.”";
"ap26_fetch02_intro_00" = "“Aku tahu ada seekor burung, berwarna biru seperti mata ibuku...";
"ap26_fetch02_intro_01" = "Ikuti <1>Orang Tua Kembang Api Bernostalgia</1> ke Arsip untuk mencari <1>Burung Biru</1>?";
"ap26_fetch02_intro_02" = "... yang kicauannya bergema dalam kenangan kita yang berharga.”";
"ap26_fetch02_mem_01" = "“Cucu kesayangan, kami membuatkan ayunan ini untukmu di pohon kesayangan kita.”";
"ap26_fetch02_mem_02" = "“Lihatlah seberapa tinggi kamu bisa naik sekarang!”";
"ap26_fetch02_mem_03" = "“Anakku, senang sekali bisa kembali ke sini bersamamu setelah sekian lama.”";
"ap26_fetch02_outro_02" = "... tetapi semakin erat kita memegangnya, semakin cepat ia menghilang.\"";
"ap26_fetch03_hint_01" = "Mungkin sang burung ingin melihat kamu tampil bersama!";
"ap26_fetch03_hint_02" = "Tapi kamu belum mengenakan pakaian untuk pertunjukan...";
"ap26_fetch03_intro_00" = "“Aku tahu ada seekor burung, berwarna biru seperti kain nila...";
"ap26_fetch03_intro_01" = "Ikuti <1>Sepupu Konfeti Berkostum</1> ke Gurun Cahaya Bintang untuk mencari <1>Burung Biru</1>?";
"ap26_fetch03_intro_02" = "...yang kicauannya menggema di mana pun orang banyak berdatangan untuk mengaguminya.";
"ap26_fetch03_outro_02" = "... tetapi terlepas dari dirinya sendiri seperti benang yang lepas.\"";
"ap26_fetch04_dialog_01" = "“Hutan yang penuh kedamaian ini adalah rumah bagi banyak burung berwarna-warni.”";
"ap26_fetch04_dialog_02" = "“Aku yakin kita bisa menemukan burung biru itu di sini!”";
"ap26_fetch04_intro_00" = "“Aku tahu ada seekor burung, berwarna biru seperti sungai yang jernih...";
"ap26_fetch04_intro_01" = "Ikuti <1>Orang Tua Tukang Kayu Memohon</1> ke Hutan Tersembunyi untuk mencari <1>Burung Biru</1>?";
"ap26_fetch04_intro_02" = "... yang kicauannya mencerminkan keharmonisan yang penuh kedamaian dari hutan.”";
"ap26_fetch04_outro_01" = "“Seekor burung ditemukan, dan bulu-bulunya berwarna biru...";
"ap26_fetch04_outro_02" = "... tetapi berhamburan tertiup angin dan hanya menyisakan keheningan.”";
"ap26_fetch05_intro_00" = "“Aku tahu seekor burung, berwarna biru seperti bintang-bintang...";
"ap26_fetch05_intro_01" = "Ikuti <1>Kakek Bijak Meramal</1> untuk mencari <1>Burung Biru</1>?";
"ap26_fetch05_intro_02" = "... yang kicauannya mengumandangkan terompet di mana takdir-takdir besar dilahirkan”";
"ap26_fetch05_outro_01" = "“Seekor burung ditemukan, dan bulu-bulunya berwarna biru...";
"ap26_fetch05_outro_02" = "... tetapi seperti sebuah takdir, hal ini tidak dimaksudkan untuk sekadar dimiliki.”";
"ap26_fetch05_quest_01" = "“Anak-anak ini tahu takdir mereka, dan mereka akan dilahirkan.”";
"ap26_fetch05_quest_02" = "“Orang lain membutuhkan bantuan dari burung biru untuk menyuarakan nasib mereka dengan lantang.”";
"ap26_fetch05_quest_03" = "“Ceritakan takdirmu, Nak.”";
"ap26_fetch05_quest_04" = "“Aku akan menjadi seorang tukang kebun dan membuat tanaman yang indah tumbuh!”";
"ap26_fetch05_quest_05" = "“Ketika aku dilahirkan, aku akan menciptakan mesin yang dapat menyembuhkan orang sakit!”";
"ap26_fetch05_quest_06" = "“Aku akan menyanyikan lagu-lagu yang memberikan harapan bagi mereka yang tidak memiliki harapan!”";
"ap26_fetch05_quest_07" = "“Mengetahui takdirmu hanyalah permulaan...”";
"ap26_fetch06_quest_01" = "Siapa yang kamu hargai lebih dari sekadar kekayaan?";
"ap26_fetch06_quest_02" = "Apa yang kamu ingat untuk mendorongmu maju?";
"ap26_fetch06_quest_03" = "Ketika kamu bercermin, diri kamu yang mana yang kamu lihat?";
"ap26_fetch06_quest_04" = "Mengapa kita mengganggu keharmonisan diri kita sendiri?";
"ap26_fetch06_quest_05" = "Berapa lama kamu akan menunggu sebelum kamu mengejar takdirmu?";
"ap26_fetch06_quest_06" = "“Di mana keberadaan Sang Burung Biru milikmu sendiri?”";
