"ap11_fetch_01_bye_01" = "Kamu jago melakukan ini!";
"ap11_fetch_01_bye_02" = "Ini kerajaan yang aneh.";
"ap11_fetch_01_bye_03" = "Aku penasaran apa ada orang dewasa di sini? ";
"ap11_fetch_01_bye_04" = "Aku harus terus menjelajah. ";
"ap11_fetch_01_chase_01" = "Ikuti aku!";
"ap11_fetch_01_chase_02" = "Tak bisa menangkapku!";
"ap11_fetch_01_chase_03" = "Ayo main.";
"ap11_fetch_01_end_quote" = "Dahulu kala ada hiduplah seorang pangeran kecil di planet yang berukuran tak lebih besar dari dirinya, dan butuh teman ...";
"ap11_fetch_01_meet_01" = "Aku suka jubahmu.";
"ap11_fetch_01_meet_02" = "Cantik sekali.";
"ap11_fetch_01_meet_03" = "Ada sesuatu yang ingin kuperlihatkan padamu.";
"ap11_fetch_01_outro_01" = "Di saat-saat seperti ini,";
"ap11_fetch_01_outro_02" = "Aku sering teringat kembali ...";
"ap11_fetch_01_outro_03" = "Temanku yang menjelajah angkasa. ";
"ap11_fetch_01_outro_04" = "Kebaikan yang ia berikan kepadaku. ";
"ap11_fetch_01_outro_05" = "Jadi, aku menunggu dia kembali ... ";
"ap11_fetch_01_play_a_01" = "Tutup matamu!";
"ap11_fetch_01_play_a_02" = "Hitung sampai lima.";
"ap11_fetch_01_play_b_01" = "Aku tidak terlalu jago, ya?";
"ap11_fetch_01_play_b_02" = "Biar kucoba lagi.";
"ap11_fetch_01_play_c_01" = "Ayo main sekali lagi. ";
"ap11_fetch_01_play_hint_01" = "Temukan Pangeran Kecil";
"ap11_fetch_01_star_01" = "Tidak jauh kok!";
"ap11_fetch_01_star_02" = "Bisa dengar mereka?";
"ap11_fetch_02_burn_01" = "Aku penasaran apakah ini mirip baobab?";
"ap11_fetch_02_burn_02" = "Baobab berduri.";
"ap11_fetch_02_butterfly_01" = "Tidak ada mawar di sini.";
"ap11_fetch_02_butterfly_02" = "Hanya kupu-kupu. Anggunnya!";
"ap11_fetch_02_butterfly_03" = "Di planetku, kami menyelamatkan dua atau tiga ulat agar jadi kupu-kupu.";
"ap11_fetch_02_butterfly_04" = "Aku tak pernah melihatnya sebanyak ini!";
"ap11_fetch_02_butterfly_05" = "Ayo terbang!";
"ap11_fetch_02_butterfly_06" = "Kita harus menjelajahi puncak!";
"ap11_fetch_02_bye_01" = "Itu tadi seru sekali!";
"ap11_fetch_02_bye_02" = "Tidak ada yang pernah puas dengan keadaan mereka ....";
"ap11_fetch_02_bye_03" = "Hanya anak-anak yang tahu keinginan mereka ...";
"ap11_fetch_02_bye_04" = "Aku punya teman yang harus dicari, dan banyak hal yang perlu dipahami.";
"ap11_fetch_02_carry_01" = "Aku senang kau di sini.";
"ap11_fetch_02_carry_02" = "Orang dewasa lupa cara menggendong belakang.";
"ap11_fetch_02_door_01" = "Menjelajah itu mudah.";
"ap11_fetch_02_door_02" = "Menemukan rumah tidak terlalu mudah.";
"ap11_fetch_02_end_quote" = "Semua orang dewasa pernah kecil ... tapi hanya sedikit dari mereka yang mengingatnya.";
"ap11_fetch_02_jelly_01" = "Apa makhluk di gua ini bernyanyi?";
"ap11_fetch_02_jelly_02" = "Aku merasa mereka akan membalas nyanyian kita.";
"ap11_fetch_02_meet_01" = "Aku tersesat ...";
"ap11_fetch_02_meet_02" = "Kau tahu jalan keluarnya?";
"ap11_fetch_02_outro_01" = "Aku masih berupa benih saat temanku menemukanku. ";
"ap11_fetch_02_outro_02" = "Baginya, aku mekar begitu indah. ";
"ap11_fetch_02_outro_03" = "Sekarang dia mencari planet baru, ";
"ap11_fetch_02_outro_04" = "tapi akankah dia menemukan mawar lain sepertiku? ";
"ap11_fetch_02_top_01" = "Aku tahu seorang pria yang tak pernah mencium bunga.";
"ap11_fetch_02_top_02" = "Dia tak pernah memandang bintang. ";
"ap11_fetch_02_top_03" = "Dia tak pernah menyayangi siapa pun. ";
"ap11_fetch_02_top_04" = "Dia tak bahkan tak pernah melakukan apa-apa di dunia selain menumpuk hartanya.";
"ap11_fetch_02_top_05" = "Dan seharian penuh dia terus menerus menyerukan ini ";
"ap11_fetch_02_top_06" = "\"Aku sibuk dengan urusan penting!\"";
"ap11_fetch_02_top_07" = "Dan itu membuat dirinya penuh dengan kesombongan.";
"ap11_fetch_02_top_08" = "Tapi dia bukan manusia. ";
"ap11_fetch_02_top_09" = "Dia adalah jamur!";
"ap11_fetch_02_whale_01" = "Ada orang di sini? ";
"ap11_fetch_02_whale_02" = "Makhluk yang anggun sekali!";
"ap11_fetch_02_whale_03" = "Apa yang satu ini bisa dijinakkan?";
"ap11_fetch_03_beach_01" = "Aku tak pernah melihat air sebanyak ini di planet-planet yang pernah kukunjungi!";
"ap11_fetch_03_beach_02" = "Andai saja aku bisa melihat apa yang terhampar di balik cakrawala.";
"ap11_fetch_03_beach_03" = "Bisakah kau antarkan aku ke puncak pulau ini? ";
"ap11_fetch_03_beach_04" = "Ayo pergi ke puncak!";
"ap11_fetch_03_belltower_01" = "Rumah yang lucu sekali!";
"ap11_fetch_03_belltower_02" = "Lebih besar dari keseluruhan planetku. ";
"ap11_fetch_03_belltower_03" = "Aku suka rumah itu bagaikan mengalunkan nyanyian bersama suara angin. ";
"ap11_fetch_03_belltower_04" = "Lihat bagaimana rumah itu dihiasi dengan lonceng. ";
"ap11_fetch_03_belltower_05" = "Seperti halnya angkasa yang bertabur hiasan bintang. ";
"ap11_fetch_03_belltower_06" = "Lonceng itu pasti untuk makhluk-makhluk. ";
"ap11_fetch_03_belltower_07" = "Semua anak kecil tahu itu. ";
"ap11_fetch_03_belltower_08" = "Ayo kita pergi menuju patung itu.";
"ap11_fetch_03_bye_01" = "Tahukah kamu ...";
"ap11_fetch_03_bye_02" = "apa artinya \"dijinakkan\"? ";
"ap11_fetch_03_bye_03" = "Seekor rubah bilang kalau itu artinya membentuk ikatan.";
"ap11_fetch_03_bye_04" = "Lihat!";
"ap11_fetch_03_bye_05" = "Jika kita membentuk ikatan, maka kita akan menjadi saling membutuhkan.";
"ap11_fetch_03_bye_06" = "Di antara jutaan anak ...";
"ap11_fetch_03_bye_07" = "Bagiku, kamu akan menjadi satu-satunya di seluruh dunia. ";
"ap11_fetch_03_bye_08" = "Bagimu, aku akan menjadi satu-satunya di seluruh dunia. ";
"ap11_fetch_03_end_quote" = "Seseorang mengambil risiko untuk merasakan sedikit kesedihan, jika ia membiarkan dirinya dijinakkan.";
"ap11_fetch_03_geyser_01" = "Ini gunung berapi!";
"ap11_fetch_03_geyser_02" = "Aku punya tiga gulung berapi, yang kubersihkan setiap minggu. ";
"ap11_fetch_03_geyser_03" = "Dua gunung berapi aktif dan satu gunung mati. ";
"ap11_fetch_03_geyser_04" = "Tapi tak ada yang tahu pastinya. ";
"ap11_fetch_03_geyser_06" = "Suara air terjun? ";
"ap11_fetch_03_geyser_07" = "Mari kita cari!";
"ap11_fetch_03_outro_01" = "Aku bersiap menghadapi harimau, ";
"ap11_fetch_03_outro_02" = "dengan duriku yang fantastis. ";
"ap11_fetch_03_outro_03" = "Tapi temanku mulai ceroboh, ";
"ap11_fetch_03_outro_04" = "saat aku goyah diterpa angin. ";
"ap11_fetch_03_outro_05" = "Akankah dia menengok kembali dan mengingatku? ";
"ap11_fetch_03_statue_01" = "Apa sebenarnya makhluk-makhluk ini? ";
"ap11_fetch_03_statue_02" = "Apa mereka bisa dijinakkan? ";
"ap11_fetch_03_statue_03" = "Sebagai kawan seumur hidup? ";
"ap11_fetch_03_statue_05" = "Suara apa itu? ";
"ap11_fetch_03_statue_06" = "Ikuti aku!";
"ap11_fetch_03_tunnel_01" = "Ayo terus menjelajah!";
"ap11_fetch_03_tunnel_02" = "Ke mana jalan ini akan berujung?";
"ap11_fetch_03_tunnel_03" = "Woah!!!";
"ap11_fetch_03_tunnel_04" = "Tadi seru sekali!";
"ap11_fetch_03_waterfall_01" = "Apa kamu mendengarnya? ";
"ap11_fetch_03_waterfall_02" = "Air terjunnya bersenandung ...";
"ap11_fetch_03_waterfall_03" = "Air mungkin juga baik untuk hati ...";
"ap11_fetch_03_waterfall_hint_01" = "Dari mana asalnya air ini?";
"ap11_fetch_03_waterfall_hint_02" = "Mari kita berjalan menelusuri asalnya.";
"ap11_fetch_04_burn01_01" = "Ada benih bagus dan benih jelek.";
"ap11_fetch_04_burn02_01" = "Benih jelek seperti baobab bisa jadi bencana bila tak cepat-cepat ditangani.";
"ap11_fetch_04_burn03_01" = "Jika itu lobak atau mawar, maka orang akan membiarkannya tumbuh di mana pun ia mau.";
"ap11_fetch_04_burn04_01" = "Bunga-bunga ini mengingatkanku akan mawarku di kejauhan.";
"ap11_fetch_04_burn05_01" = "Bunga itu lembut. ";
"ap11_fetch_04_burn05_02" = "Terkadang kamu perlu melindunginya dari angin.";
"ap11_fetch_04_burn06_01" = "Aku mendengarkan mawarku saat dia menggerutu, atau membual ...";
"ap11_fetch_04_burn06_02" = "atau bahkan saat dirinya tak bicara satu patah kata pun.";
"ap11_fetch_04_burn07_01" = "Waktu yang kamu habiskan menyirami mawarmu ...";
"ap11_fetch_04_burn07_02" = "Adalah alasan mengapa mereka begitu penting.";
"ap11_fetch_04_burn_start_hint" = "Hilangkan kegelapan untuk merawat bunga-bunga.";
"ap11_fetch_04_bye01_01" = "Mawar-mawar ini indah.";
"ap11_fetch_04_bye01_02" = "Tetapi mereka hampa.";
"ap11_fetch_04_bye01_03" = "Mawarku adalah satu-satunya bagiku di seluruh dunia.";
"ap11_fetch_04_bye01_04" = "Mawarku mewangikan seluruh planetku ...";
"ap11_fetch_04_bye01_05" = "Tapi aku tidak tahu cara menikmati semua anugerahnya.";
"ap11_fetch_04_bye02_01" = "Rubahku pernah bilang ...";
"ap11_fetch_04_bye02_02" = "Seseorang hanya bisa melihat jelas dengan hatinya. ";
"ap11_fetch_04_bye02_03" = "Segala yang penting tak tampak oleh mata. ";
"ap11_fetch_04_end_quote" = "Bintang-bintang indah karena bunga yang tak terlihat olehmu ...";
"ap11_fetch_04_meet_01" = "Ada yang aneh di sana.";
"ap11_fetch_04_meet_02" = "Bunga-bunga ini kekurangan udara!";
"ap11_fetch_04_outro_01" = "Tawanya menjadi sunyi dalam sekejap. ";
"ap11_fetch_04_outro_02" = "Kesombonganku terkalahkan. ";
"ap11_fetch_04_outro_03" = "Tanpa kawanku di sisi,";
"ap11_fetch_04_outro_04" = "matahari terbenam ini terasa kurang manis. ";
"ap11_fetch_04_outro_05" = "Aku ingin tahu apa dia akan merasakan penyesalan suatu hari nanti? ";
"ap11_fetch_05_bye_01" = "Terima kasih ...";
"ap11_fetch_05_bye_02" = "Aku senang punya teman sepertimu. ";
"ap11_fetch_05_end_quote" = "Seseorang bertanggung jawab selamanya atas apa yang telah dijinakkannya.";
"ap11_fetch_05_meet_01" = "Aku menemukan ini di gua yang kita kunjungi.";
"ap11_fetch_05_meet_02" = "Bagaimana cara kerjanya?";
"ap11_fetch_05_meet_03" = "Mau coba ini untukku? ";
"ap11_fetch_05_outro_01" = "(batuk)";
"ap11_fetch_05_outro_02" = "Beberapa hari, aku mengharumkan planetnya. ";
"ap11_fetch_05_outro_03" = "Beberapa malam, aku layu dalam kepura-puraan. ";
"ap11_fetch_05_outro_04" = "Tapi dia tak bisa melihat persahabatanku, ";
"ap11_fetch_05_outro_05" = "atau kebahagiaan dalam jiwaku. ";
"ap11_fetch_05_outro_06" = "Akankah dia tahu ...? ";
"ap11_fetch_05_reflect_01" = "Seharusnya aku menilainya dari tindakannya ...";
"ap11_fetch_05_reflect_02" = "bukan dari perkataannya. ";
"ap11_fetch_05_reflect_04" = "Menurutmu aku bertanggung jawab atas mawarku?";
"ap11_fetch_05_sunset_01" = "Aku suka sekali matahari terbenam. ";
"ap11_fetch_05_sunset_02" = "Seperti saat di rumah. ";
"ap11_fetch_05_sunset_03" = "Suatu hari, aku melihat matahari terbenam empat puluh empat kali!";
"ap11_fetch_05_sunset_04" = "Aku ingin tahu, apakah mawarku masih melihat matahari terbenam? ";
"ap11_fetch_05_sunset_05" = "Aku ingin tahu, apakah dia masih aman dari angin? ";
"ap11_fetch_05_sunset_07" = "Bisakah kau mainkan musik untukku lagi? ";
"ap11_fetch_06_campfire_01" = "Aku akan membuatkan hadiah untukmu ...";
"ap11_fetch_06_campfire_02" = "Itu hadiahku. Itu saja.";
"ap11_fetch_06_campfire_03" = "Aku akan pulang malam ini.";
"ap11_fetch_06_campfire_04" = "Di salah satu bintang-bintang itu aku akan berada.";
"ap11_fetch_06_campfire_05" = "Di salah satunya pula aku akan tertawa. ";
"ap11_fetch_06_campfire_06" = "Dan saat kau memandang langit kala malam ...";
"ap11_fetch_06_campfire_07" = "akan terasa seolah semua bintang tertawa.";
"ap11_fetch_06_campfire_08" = "Tempatnya sangat jauh dan akan berat ...";
"ap11_fetch_06_campfire_09" = "Aku akan jauh lebih takut.";
"ap11_fetch_06_campfire_10" = "Akan tampak seakan-akan aku menderita.";
"ap11_fetch_06_campfire_11" = "Jangan datang untuk melihat itu.";
"ap11_fetch_06_campfire_12" = "Di malam hari kau akan melihat bintang.";
"ap11_fetch_06_campfire_13" = "Bintangku akan jadi salah satu bintang untukmu.";
"ap11_fetch_06_campfire_14" = "Dan supaya kau akan senang melihat semua bintang di langit ...";
"ap11_fetch_06_campfire_15" = "Mereka semua akan jadi temanmu.";
"ap11_fetch_06_end_quote" = "Cara menggapainya? Tempat menemukannya? Masih menjadi misteri, negeri air mata.";
"ap11_fetch_06_outro_01" = "(batuk-batuk)";
"ap11_fetch_06_outro_02" = "Waktuku sudah habis ... ";
"ap11_fetch_06_outro_03" = "Aku minta maaf sedalam-dalamnya. ";
"ap11_fetch_06_outro_04" = "Tapi, dia belum kembali jua. ";
"ap11_fetch_06_outro_05" = "Akankah dia kembali untukku? ";
"ap11_fetch_07_end_quote" = "Bintangku akan jadi salah satu bintang untukmu. Dan supaya kau akan senang melihat semua bintang di langit. Mereka semua akan jadi temanmu. ";
"ap11_fetch_07_end_quote_01" = "Bintangku akan jadi salah satu bintang untukmu. ";
