"sf_hist_03_04" = "Perencanaan Seni";
"sf_hist_04_00" = "Selama beberapa tahun pertama pengembangan, studio ini melakukan iterasi melalui eksplorasi dan penemuan yang menyenangkan. Lompatan ke pengembangan mobile, game yang dapat dimainkan secara gratis, dan menerbitkan sendiri sebuah judul game yang dapat dimainkan secara real-time menantang tim dan memperpanjang jadwal produksi.";
"sf_hist_04_01" = "Bereksperimen dengan Desain Suara";
"sf_hist_04_02" = "Diskusi Seni Lingkungan";
"sf_hist_04_05" = "Tim seni mempelajari banyak bentuk awan dan perilakunya untuk mendesain dunia di sky.";
"sf_hist_04_06" = "Inspirasi datang dari berbagai emosi yang disampaikan awan pada waktu yang berbeda sepanjang hari.";
"sf_hist_04_07" = "Prototipe awal untuk pengujian.";
"sf_hist_04_08" = "Beberapa prototipe dibuat untuk membantu merencanakan teknologi dan mesin permainan yang dibutuhkan untuk lanskap awan dan penerbangan.";
"sf_hist_04_09" = "Terbukti bahwa setelah ditempatkan di antara awan, kami harus mengandalkan awan sebagai latar belakang dan pemandangan yang emosional.";
"sf_hist_04_10" = "Memperkenalkan lingkungan yang sudah dikenal.";
"sf_hist_04_11" = "Dunia manusia di Sky.";
"sf_hist_04_12" = "Cakrawala di atas lanskap kota manusia untuk menguji penerbangan.";
