"ap14_story_tree_0" = "Dahulu kala...";
"ap14_story_tree_1a" = "Terdapat sebuah bintang yang berkilauan";
"ap14_story_tree_1a2a" = "Bintang itu semakin benderang";
"ap14_story_tree_1a2a3a" = "Bintang itu tak bisa lagi menahan cahaya dalam dirinya";
"ap14_story_tree_1a2a3a4a" = "Bintang itu hancur, memberikan pancaran yang dibangunnya selama ribuan tahun kepada yang lainnya";
"ap14_story_tree_1a2a3a4a_abv" = "Bintang itu hancur...";
"ap14_story_tree_1a2a3a4a_cont" = "Langit malam berdenyut terang. Di mana satu bintang bersinar, cahaya-cahaya kecil lain yang tak terhitung jumlahnya turut muncul.";
"ap14_story_tree_1a2a3a4a_cont_abv" = "Langit malam berdenyut terang...";
"ap14_story_tree_1a2a3a4b" = "Bintang itu memberikan cahayanya kepada seorang anak kesepian yang berkeliaran di padang pasir";
"ap14_story_tree_1a2a3a4b_abv" = "Bintang itu memberikan cahayanya...";
"ap14_story_tree_1a2a3a4b_cont" = "Dipandu cahayanya, anak itu melihat jalan di hadapannya. Mulai saat itu, dia tak akan pernah sendirian.";
"ap14_story_tree_1a2a3a4b_cont_abv" = "Dipandu cahayanya...";
"ap14_story_tree_1a2a3a_abv" = "Bintang itu tak bisa lagi menahan...";
"ap14_story_tree_1a2a3a_cont" = "Dalam tindakan tanpa pamrih...";
"ap14_story_tree_1a2a3b" = "Cahayanya menyelimuti seluruh langit";
"ap14_story_tree_1a2a3b4a" = "Tak ada yang bisa ia lihat selain kilau sinarnya yang menyilaukan";
"ap14_story_tree_1a2a3b4a_abv" = "Tak ada yang bisa ia lihat...";
"ap14_story_tree_1a2a3b4a_cont" = "Diam dan sendirian, bintang itu meredupkan cahayanya sampai sosok-sosok itu kembali. Ia tahu, bila tidak ada yang ia sinari, maka pancarannya hanya sia-sia.";
"ap14_story_tree_1a2a3b4a_cont_abv" = "Diam dan sendirian...";
"ap14_story_tree_1a2a3b4b" = "Bintang itu melihat seorang anak mengembara di gurun yang diterangi cahaya dengan penuh percaya diri";
"ap14_story_tree_1a2a3b4b_abv" = "Bintang itu melihat...";
"ap14_story_tree_1a2a3b4b_cont" = "Dengan cahaya penuh kebanggaan, bintang tersebut memancarkan sinarnya yang indah sampai anak itu menemukan tempat berlindung di sebuah kuil. Bersyukur bisa membantu, akhirnya bintang itu bisa beristirahat.";
"ap14_story_tree_1a2a3b4b_cont_abv" = "Dengan cahaya penuh kebanggaan...";
"ap14_story_tree_1a2a3b_abv" = "Cahayanya menyelimuti...";
"ap14_story_tree_1a2a3b_cont" = "Saat akhirnya bintang itu melihat ke bawah...";
"ap14_story_tree_1a2a_cont" = "Hingga...";
"ap14_story_tree_1a2b" = "Cahayanya semakin redup";
"ap14_story_tree_1a2b3a" = "Cahayanya hampir lenyap dari langit";
"ap14_story_tree_1a2b3a4a" = "Cahayanya terbelah menjadi titik-titik cahaya yang tak terhitung jumlahnya, dengan lembut turun ke tanah di bawahnya.";
"ap14_story_tree_1a2b3a4a_abv" = "Cahayanya terbelah...";
"ap14_story_tree_1a2b3a4a_cont" = "Dari gaungannya muncul makhluk-makhluk cahaya yang indah beterbangan. Akhir dari satu kehidupan memicu kehidupan baru yang lain.";
"ap14_story_tree_1a2b3a4a_cont_abv" = "Dari gaungannya...";
"ap14_story_tree_1a2b3a4b" = "Bintang itu mulai jatuh ke tanah, bersinar jauh lebih terang dari rembulan.";
"ap14_story_tree_1a2b3a4b_abv" = "Bintang itu mulai jatuh...";
"ap14_story_tree_1a2b3a4b_cont" = "Seorang anak muncul, sebuah bintang telah berubah.";
"ap14_story_tree_1a2b3a4b_cont_abv" = "Seorang anak muncul...";
"ap14_story_tree_1a2b3a_cont" = "Tapi sebelum benar-benar menghilang...";
"ap14_story_tree_1a2b3b" = "Seorang anak mengungkapkan harapnya";
"ap14_story_tree_1a2b3b4a" = "Bintang itu mengerahkan semua energinya untuk berkedip lemah terakhir kalinya.";
"ap14_story_tree_1a2b3b4a_abv" = "Bintang itu mengerahkan semua energinya...";
"ap14_story_tree_1a2b3b4a_cont" = "Menyaksikan kilau terakhirnya, anak itu tidak lagi merasa sendirian. Sekarang penuh dengan harapan, bahwa mereka bisa bertahan.";
"ap14_story_tree_1a2b3b4a_cont_abv" = "Menyaksikan kilau terakhirnya...";
"ap14_story_tree_1a2b3b4b" = "Bintang itu mendedikasikan cahaya terakhirnya untuk membantu anak yang mengembara itu.";
"ap14_story_tree_1a2b3b4b_abv" = "Bintang itu mendedikasikan...";
"ap14_story_tree_1a2b3b4b_cont" = "Menyinari gua, bintang itu memandu anak tersebut mencari tempat perlindungan...seperti yang diramalkan nubuat.";
"ap14_story_tree_1a2b3b4b_cont_abv" = "Menyinari gua...";
"ap14_story_tree_1a2b3b_cont" = "Mendengar panggilan mereka...";
"ap14_story_tree_1a2b_cont" = "Hingga...";
"ap14_story_tree_1a_cont" = "Setiap malam...";
"ap14_story_tree_1b" = "Hiduplah seorang anak pendiam";
"ap14_story_tree_1b2a" = "Mereka mencari teman";
"ap14_story_tree_1b2a3a" = "Kaki mereka yang lelah sudah tak sanggup membawa diri mereka lebih jauh";
"ap14_story_tree_1b2a3a4a" = "Mereka melihat anak lain masuk sebuah gua sederhana.";
"ap14_story_tree_1b2a3a4a_abv" = "Mereka melihat anak lain...";
"ap14_story_tree_1b2a3a4a_cont" = "Anak-anak itu menemukan persahabatan dari satu sama lain, membuat gua menjadi terasa seperti rumah. Suatu saat nanti, mereka pasti akan menyambut kedatangan anak yang lainnya.";
"ap14_story_tree_1b2a3a4a_cont_abv" = "Anak-anak itu menemukan persahabatan...";
"ap14_story_tree_1b2a3a4b" = "Mereka menyadari pola di batu itu mulai bersinar.";
"ap14_story_tree_1b2a3a4b_abv" = "Mereka menyadari pola...";
"ap14_story_tree_1b2a3a4b_cont" = "Pola tersebut memperlihatkan tiga sosok yang mengembara menggunakan perahu. Anak itu ditakdirkan menjadi yang keempat.";
"ap14_story_tree_1b2a3a4b_cont_abv" = "Pola tersebut memperlihatkan...";
"ap14_story_tree_1b2a3a_abv" = "Kaki mereka yang lelah...";
"ap14_story_tree_1b2a3a_cont" = "Beristirahat di bawah naungan formasi batuan yang aneh...";
"ap14_story_tree_1b2a3a_cont_abv" = "Beristirahat di bawah naungan...";
