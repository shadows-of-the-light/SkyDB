"sf_art_08_00" = "Nelle fasi iniziali, i \"custodi animali\" venivano delineati come figure mitiche che vegliavano sui loro reami. Si sono evoluti lentamente nei personaggi umanoidi degli anziani antenati che consideriamo parte integrante dell'ascesa e della caduta del regno.";
"sf_art_08_01" = "Insieme, erano guidati da un Sovrano, che alla fine si era sacrificato per dare vita a una nuova generazione di Figli della Luce.\n(<b>Opera di Jacky Ke Jiang</b>)";
"sf_art_08_02" = "ISOLA DELL'ALBA - SAGGEZZA\n(<b>Opera di Tom Zhao</b>)";
"sf_art_08_03" = "PRATERIA DIURNA - EMPATIA\n(<b>Opera di Tom Zhao</b>)";
"sf_art_08_04" = "FORESTA NASCOSTA - RESILIENZA\n(<b>Opera di Tom Zhao</b>)";
"sf_art_09_00" = "Il team artistico ha esplorato i momenti narrativi cruciali in cui ogni antenato avrebbe incontrato i Figli della Luce e ha studiato come catturarli visivamente per creare un maggiore impatto.";
"sf_art_09_01" = "VALLE DEL TRIONFO - ECCELLENZA\n(<b>Opera di Tom Zhao</b>)";
"sf_art_09_02" = "DESERTO DORATO - CORAGGIO\n(<b>Opera di Tom Zhao</b>)";
"sf_art_09_03" = "CUPOLA DELLA CONOSCENZA - FEDE\n(<b>Opera di Tom Zhao</b>)";
"sf_art_09_04" = "IL SOVRANO - SACRIFICIO\n(<b>Opera di Khang Le</b>)";
"sf_art_10_01" = "Nello sviluppare il retroscena, abbiamo delineato il possibile scopo della vita degli abitanti di questa civiltà.\n(<b>Schizzo di Yuichiro Tanabe</b>)";
"sf_art_10_02" = "(<b>Schizzo di Yuichiro Tanabe</b>)";
"sf_art_10_03" = "(<b>Schizzo di Yuichiro Tanabe</b>)";
"sf_art_10_04" = "In principio, gli abitanti dei villaggi di ogni reame adoravano il ciclo dell'anima come parte della coesistenza tra umanità e natura.\n(<b>Schizzo di Yuichiro Tanabe</b>)";
"sf_art_10_05" = "(<b>Schizzo di Yuichiro Tanabe</b>)";
"sf_art_10_06" = "(<b>Opera d'arte di Jacky Ke Jiang, Tom Zhao</b>)";
"sf_art_11_00" = "Gli spiriti ancestrali fornivano un legame con le civiltà del passato e i loro ricordi offrono consistenza e sapore a ogni reame, con vari livelli di sapere e scoperte.";
"sf_art_11_01" = "ISOLA DELL'ALBA\n(<b>Opera di Yuichiro Tanabe</b>)";
"sf_art_11_02" = "PRATERIA DIURNA\n(<b>Opera di Yuichiro Tanabe</b>)";
