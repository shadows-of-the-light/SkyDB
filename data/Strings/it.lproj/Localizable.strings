"ap11_fetch_01_chase_02" = "Non mi prendi!";
"ap11_fetch_01_chase_03" = "Facciamo un gioco.";
"ap11_fetch_01_end_quote" = "C'era una volta un piccolo principe che viveva su di un pianeta poco più grande di lui e aveva bisogno di un amico...";
"ap11_fetch_01_meet_01" = "Bello il tuo mantello.";
"ap11_fetch_01_meet_02" = "È davvero bello.";
"ap11_fetch_01_meet_03" = "Vorrei mostrarti una cosa.";
"ap11_fetch_01_outro_01" = "In giorni come questi,";
"ap11_fetch_01_outro_02" = "Ripenso con affetto...";
"ap11_fetch_01_outro_03" = "Al mio amico che esplora i cieli.";
"ap11_fetch_01_outro_04" = "Con quanta gentilezza si prendeva cura di me.";
"ap11_fetch_01_outro_05" = "E così, attendo il suo ritorno...";
"ap11_fetch_01_play_a_01" = "Chiudi gli occhi!";
"ap11_fetch_01_play_a_02" = "Conta fino a cinque.";
"ap11_fetch_01_play_b_01" = "Non sono granché...";
"ap11_fetch_01_play_b_02" = "Fammi riprovare.";
"ap11_fetch_01_play_c_01" = "Giochiamo di nuovo.";
"ap11_fetch_01_play_hint_01" = "Trova il Piccolo Principe";
"ap11_fetch_01_star_01" = "Non è lontana!";
"ap11_fetch_01_star_02" = "Le senti?";
"ap11_fetch_02_burn_01" = "Questi sono come baobab?";
