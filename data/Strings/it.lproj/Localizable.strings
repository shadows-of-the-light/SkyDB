"ap24_credits_01" = "Grazie per aver giocato...";
"ap24_credits_02" = "Stagione dei Mumin";
"ap24_credits_03" = "Basato su \"La bambina invisibile\"";
"ap24_credits_04" = "di Tove Jansson";
"ap24_credits_05" = "Con scene selezionate dai libri";
"ap24_credits_06" = "\"Caccia alla cometa\"";
"ap24_credits_07" = "\"Il cappello del Gran Bau\"";
"ap24_credits_08" = "e \"Racconti dalla Valle dei Mumin\"";
"ap24_fq01_chat_01" = "Ti porto nella tua nuova casa.";
"ap24_fq01_chat_02" = "Dai, vieni!";
"ap24_fq01_chat_03" = "Ci siamo quasi.";
"ap24_fq01_chat_04" = "Dai, vieni!";
"ap24_fq01_chat_05" = "Chi ci hai portato?";
"ap24_fq01_chat_06" = "Si chiama Ninny.";
"ap24_fq01_chat_07" = "Era spaventata da una signora che si prendeva cura di lei, ma non le voleva bene...";
"ap24_fq01_chat_08" = "e così è diventata invisibile.";
"ap24_fq01_chat_09" = "Ho incontrato quella donna ed era orribile, aveva un atteggiamento gelidamente sarcastico.";
"ap24_fq01_chat_10" = "Terribile!";
"ap24_fq01_chat_11" = "Se le persone si spaventano molto spesso, a volte diventano invisibili.";
"ap24_fq01_chat_12" = "Dai una sedia alla bambina.";
"ap24_fq01_chat_13" = "Questa è la tua nuova famiglia!";
"ap24_fq01_story_01" = "Una sera piovosa, una persona gentile stava conducendo una bambina invisibile attraverso la valle dei Mumin.";
"ap24_fq01_story_02" = "Si chiamava Dede.";
"ap24_fq01_story_03" = "Forse i suoi amici avrebbero potuto aiutare la bambina a tornare visibile.";
"ap24_fq02_chat_01" = "Dormi quanto vuoi. Domattina la colazione sarà pronta quando lo vorrai tu.";
"ap24_fq02_story_01" = "Il campanello d'argento tintinnò alle spalle di Mamma Mumin al piano di sopra.";
"ap24_fq02_story_02" = "Sogni d'oro, Ninny.";
"ap24_fq03_chat_01" = "Mamma! Ha le zampe! Sono comparse le zampe!";
"ap24_fq03_chat_02" = "Magnifico! E sarà ancora meglio quando un giorno potremo vedere il suo musetto.";
"ap24_fq03_chat_03" = "Raccoglievano le mele. Era una splendida mattina d'autunno.";
"ap24_fq03_chat_04" = "Brava, Ninny!";
"ap24_fq03_chat_05" = "Oh, quello era il barattolo che usavamo per dare il cibo ai bombi.";
"ap24_fq03_chat_06" = "Ora non dobbiamo più portarlo al campo.";
"ap24_fq03_chat_07" = "Vedo le sue gambe!";
"ap24_fq04_chat_01" = "Grazie mille a tutti.";
"ap24_fq04_chat_02" = "Siamo felici di poterti vedere un po' di più oggi!";
"ap24_fq04_chat_03" = "E hai anche iniziato a parlare. Conosci qualche gioco carino?";
"ap24_fq04_chat_04" = "No...";
"ap24_fq04_chat_05" = "Però ho sentito parlare dei giochi!";
"ap24_fq04_story_01" = "Quella notte, Mamma Mumin fece a Ninny un vestito rosa e lo sistemò nella soffitta orientale.";
"ap24_fq04_story_02" = "Ninny scese a fare colazione con la famiglia Mumin.";
"ap24_fq05_chat_01" = "Ninny era visibile fino al collo, soltanto il suo viso era ancora invisibile.";
"ap24_fq05_chat_02" = "Il Mumin era felicissimo. Decise di insegnare a Ninny tutti i giochi che conosceva.";
"ap24_fq05_chat_03" = "Sali, Ninny!";
"ap24_fq05_chat_04" = "Mi dispiace, non posso...";
"ap24_fq05_chat_05" = "Corri, corri, dai!";
"ap24_fq05_chat_06" = "Scusa...";
"ap24_fq05_chat_07" = "Non sai saltare?";
"ap24_fq05_chat_08" = "Era chiaro a tutti che giocava solo per cortesia e non per divertirsi.";
"ap24_fq05_chat_09" = "Mi dispiace tanto...";
"ap24_fq05_chat_10" = "Pensi che qualcuno apprezzi ciò che stai facendo?";
"ap24_fq05_chat_11" = "Non hai un briciolo di entusiasmo?";
"ap24_fq05_chat_12" = "Vuoi una botta sul naso?";
"ap24_fq05_chat_13" = "Non sa giocare...";
"ap24_fq05_chat_14" = "Non sa arrabbiarsi.";
"ap24_fq05_chat_15" = "Ecco cosa c'è che non va in lei.";
"ap24_fq05_chat_16" = "Non avrai mai un volto finché non imparerai a combattere!";
"ap24_fq05_chat_17" = "Sì, certo.";
"ap24_fq06_chat_01" = "Cos'ha Ninny?";
"ap24_fq06_chat_01b" = "È spaventata?";
"ap24_fq06_chat_02" = "Sì, Ninny pensa che il mare sia troppo grande.";
"ap24_fq06_chat_03" = "Ciao.";
"ap24_fq06_chat_04" = "Come sta la bambina invisibile?";
"ap24_fq06_chat_05" = "Soltanto la testa è ancora invisibile.";
"ap24_fq06_chat_06" = "Puoi darci una mano con la barca?";
"ap24_fq06_chat_07" = "Oddio, l'acqua sembra gelida!";
"ap24_fq06_chat_08" = "Per settimane non era successo niente di entusiasmante.";
"ap24_fq06_chat_09" = "Ottimo lavoro! ";
"ap24_fq06_chat_10" = "Io non avrei potuto fare di meglio!";
"ap24_fq06_chat_11" = "La vedo, la vedo!";
"ap24_fq06_chat_12" = "Non osare spingerla in questo grande mare spaventoso!";
"ap24_fq06_story_01" = "Passarono i giorni e Ninny era ancora senza volto.";
"ap24_fq06_story_02" = "Un giorno la famiglia andò alla spiaggia a tirare a riva la barca per l'inverno.";
"ap24_fq06_story_03" = "Quando giunsero in vista del mare, Ninny si fermò all'improvviso.";
"ap24_fq06_story_04" = "Papà Mumin ebbe un'idea scherzosa e cominciò ad avvicinarsi di soppiatto a mamma Mumin.";
"ap24_fq06_story_05" = "Non voleva veramente spingerla in acqua, era uno scherzo per far ridere un po' i bambini.";
"ap24_name_little" = "Dada";
"ap24_name_moominmamma" = "Mamma Mumin";
"ap24_name_moominpappa" = "Papà Mumin";
"ap24_name_moomintroll" = "Mumin";
"ap24_name_ninny" = "Ninny";
"ap24_name_sniff" = "Sniffa";
"ap24_name_snufkin" = "Wilson";
"ap24_name_too_ticky" = "Dede";
"ap24_side01_chat_01" = "È la serata giusta per una melodia.";
"ap24_side01_chat_02" = "Una nuova melodia.";
"ap24_side01_chat_03" = "Una parte di aspettative...";
"ap24_side01_chat_04" = "due parti di malinconia primaverile...";
"ap24_side01_chat_05" = "e per il resto...";
"ap24_side01_chat_06" = "solo il grande piacere di godersi un po' di solitudine!";
"ap24_side02_chat_01" = "Ci sedevamo proprio così in primavera.";
"ap24_side02_chat_02" = "Te lo ricordi?";
"ap24_side02_chat_03" = "Ci eravamo svegliati dal letargo invernale ed era il primo giorno.";
"ap24_side02_chat_04" = "Tutti gli altri dormivano ancora.";
"ap24_side02_chat_05" = "Hai parlato di progetti.";
"ap24_side02_chat_06" = "Ne hai qualcuno anche tu?";
"ap24_side02_chat_07" = "Sì, ho un progetto.";
"ap24_side02_chat_08" = "Però è una cosa solitaria.";
"ap24_side02_chat_09" = "Stai pensando di andartene.";
"ap24_side02_chat_10" = "Quando parti?";
"ap24_side02_chat_11" = "Adesso, subito!";
"ap24_side02_chat_12" = "Sarai via a lungo?";
"ap24_side02_chat_13" = "No, il primo giorno di primavera sarò di nuovo qui a fischiare sotto la tua finestra.";
"ap24_side02_chat_14" = "Un anno passa velocemente!";
"ap24_side03_chat_01" = "Non è che per caso sei un pittore?";
"ap24_side03_chat_02" = "O magari un poeta?";
"ap24_side03_chat_03" = "Io sono tutto!";
"ap24_side03_chat_04" = "E vedo che voi siete esploratori.";
"ap24_side03_chat_05" = "Cosa pensate di scoprire?";
"ap24_side03_chat_06" = "Oh, tutto.";
"ap24_side03_chat_07" = "Le stelle, per esempio!";
"ap24_side03_chat_08" = "Le stelle!";
"ap24_side03_chat_09" = "Allora devo venire con voi.";
"ap24_side03_chat_10" = "Adoro le stelle.";
"ap24_side03_chat_11" = "Quando mi metto a letto le guardo sempre prima di addormentarmi...";
"ap24_side03_chat_12" = "e mi chiedo da chi siano abitate.";
"ap24_side04_chat_01" = "Sigh...";
"ap24_side04_chat_02" = "Mi sembri giù.";
"ap24_side04_chat_03" = "Ho regalato il mio giocattolo preferito.";
"ap24_side04_chat_04" = "Pensavo che sarebbe stato bello fare qualcosa di carino.";
"ap24_side04_chat_05" = "Ah. Te ne penti?";
"ap24_side04_chat_06" = "Sì.";
"ap24_side04_chat_07" = "Hai perso il giocattolo...";
"ap24_side04_chat_08" = "ma forse hai guadagnato qualcosa di più importante.";
"ap24_side06_chat_01" = "Il loro segreto ha a che fare con i temporali!";
"ap24_side06_chat_02" = "Li cercano e li bramano sempre...";
"ap24_side06_chat_03" = "Cosa mi è successo?";
"ap24_side06_chat_04" = "Non sono un Fungarello, sono Papà Mumin...";
"ap24_side06_chat_05" = "Cosa ci faccio qui?";
"ap24_side06_chat_06" = "Vado a casa! Me ne vado subito!";
"ap24_side07_story_01" = "Una volta, Papà Mumin se ne andò di casa senza la minima spiegazione.";
"ap24_side07_story_02" = "Nessuno sapeva con certezza dove fosse andato.";
"ap24_side07_story_03" = "Mai nella sua vita Papà Mumin si era sentito così a suo agio.";
"ap24_side07_story_04" = "Amava il fatto di non dover dire nulla...";
"ap24_side07_story_05" = "o spiegare alcunché, a se stesso o agli altri.";
"ap24_side07_story_06" = "Poteva semplicemente sedersi a guardare l'orizzonte...";
"ap24_side07_story_07" = "e ascoltare il suono dell'acqua.";
"ap24_storybook_hint01" = "Vuoi diventare una farfalla e recarti nella <1>Valle dei Mumin</1>?";
"ap24_storybook_hint02" = "Vuoi esplorare liberamente la <1>Valle dei Mumin</1> come una farfalla?";
"ap24_storybook_hint03" = "Vuoi recarti nella <1>Valle dei Mumin</1>?";
