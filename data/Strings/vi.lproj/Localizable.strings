"sf_art_01_00" = "Sky ban đầu được tạo ra như là phần tiếp theo của Cloud (một trò chơi được thiết kế trước khi studio thatgamecompany được thành lập).\n\nSky bắt đầu phát triển vào năm 2012 bởi một đội ngũ khoảng 10 nhà phát triển tại văn phòng ở Santa Monica, California.";
"sf_art_01_01" = "Hình ảnh đầu tiên của Sky là một bức phác họa bằng bút chì nổi bật về những đứa trẻ bay cùng nhau, tâm hồn chúng hòa quyện vào nhau trong hình dáng của một con chim thần thoại.\n(<b>Bản phác thảo của Jenova Chen</b>)";
"sf_art_01_02" = "Bức vẽ The Giant của nghệ sĩ người Mỹ N.C. Wyeth là nguồn cảm hứng trung tâm của Sky ngay từ những ngày đầu.\n(<b>Bức vẽ The Giant của N.C. Wyeth</b>)";
"sf_art_01_03" = "Chủ đề câu chuyện của Hoàng Tử Bé đã trở thành nguồn tham khảo trong quá trình phát triển ban đầu.\n(<b>Hình ảnh Hoàng Tử Bé, tác giả Antoine de Saint-Exupéry</b>)";
"sf_art_01_04" = "Phim tài liệu Winged Migration đã cho chúng tôi biết về các trạng thái khác nhau do các loài chim bay đường dài tạo ra.\n(<b>Đạo diễn Jacques Perrin, Jacques Cluzaud, Michel Debats</b>)";
"sf_art_02_01" = "“Overview Effect” là một video có sức ảnh hưởng lớn được chia sẻ bởi các phi hành gia từ trạm vũ trụ. Họ nói về cảm giác kinh ngạc mà họ cảm nhận được khi nhìn ngắm trái đất. Điều này đã truyền cảm hứng cho một bức tranh ban đầu ở giai đoạn tiền sản xuất.\n(<b>Tác phẩm nghệ thuật của Jenova Chen</b>)";
"sf_art_03_00" = "Trong quá trình khám phá nghệ thuật ban đầu, các bản phác thảo tâm trạng đầu tiên của chúng tôi dựa trên ý tưởng về một đứa trẻ bay lên bầu trời để gặp được những người khổng lồ.\n\nTrong giai đoạn này, chúng tôi đã vẽ nhiều hòn đảo mà bạn có thể lướt qua.";
"sf_art_03_01" = "Những bức tranh này toát lên vẻ huyền ảo và trẻ thơ.\n(<b>Tác phẩm nghệ thuật của Jacky Ke Jiang</b>)";
"sf_art_03_02" = "Chúng tôi đã tưởng tượng ra con đường của những linh hồn gặp được những người khổng lồ và sinh vật kỳ diệu giữa những đám mây.\n(<b>Tác phẩm nghệ thuật của Jacky Ke Jiang</b>)";
"sf_art_03_03" = "Sự tự do và cảm giác bay lượn trên những hòn đảo xanh mướt và đại dương đã trở thành mục tiêu phát triển quan trọng.\n(<b>Tác phẩm nghệ thuật của Yuichiro Tanabe</b>)";
"sf_art_03_04" = "(<b>Tác phẩm nghệ thuật của Yuichiro Tanabe</b>)";
"sf_art_03_05" = "(<b>Tác phẩm nghệ thuật của Tiffany Hayashi, Yuichiro Tanabe</b>)";
"sf_art_04_00" = "Đội ngũ chúng tôi nhận ra rằng chủ đề “kết nối” sẽ là yếu tố cốt lõi của trò chơi. Chúng tôi đã tạo ra một nguyên mẫu để thử nghiệm những cách kết nối khác nhau, bao gồm tùy chọn cho phép người chơi nắm tay nhau. Lần chơi thử này đã trở thành một cột mốc quan trọng trong quá trình phát triển trò chơi của chúng tôi.";
"sf_art_04_01" = "(<b>Tác phẩm nghệ thuật của Yuichiro Tanabe, Jacky Ke Jiang</b>)";
"sf_art_05_01" = "(<b>Tác phẩm nghệ thuật của Tom Zhao</b>)";
