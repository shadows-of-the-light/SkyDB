"ap24_fq01_chat_01" = "Ta sẽ đưa con đến ngôi nhà mới của mình.";
"ap24_fq01_chat_02" = "Đi nào!";
"ap24_fq01_chat_03" = "Chúng ta sắp đến nơi rồi.";
"ap24_fq01_chat_04" = "Nào, đi thôi!";
"ap24_fq01_chat_05" = "Cô đưa ai đến vậy?";
"ap24_fq01_chat_06" = "Tên cô ấy là Ninny.";
"ap24_fq01_chat_07" = "Cô bé e sợ người phụ nữ đã chăm sóc mình, bà ta thực sự không hề ưa cô bé...";
"ap24_fq01_chat_08" = "...và cô bé đã trở thành vô hình.";
"ap24_fq01_chat_09" = "Tôi đã gặp người phụ nữ đó, và bà ta thật đáng sợ - kiểu người lạnh lùng và mỉa mai.";
"ap24_fq01_chat_10" = "Thật khó chịu!";
"ap24_fq01_chat_11" = "Nếu bị làm cho sợ hãi quá nhiều lần, đôi khi con người ta sẽ trở thành vô hình.";
"ap24_fq01_chat_12" = "Hãy đưa cho đứa bé một chiếc ghế.";
"ap24_fq01_chat_13" = "Đây là gia đình mới của con!";
"ap24_fq01_story_01" = "Vào một buổi tối mưa, một nhân vật tốt bụng đang dẫn một cô bé vô hình đi qua Moominvalley.";
"ap24_fq01_story_02" = "Người phụ nữ đó tên là Too-ticky.";
"ap24_fq01_story_03" = "Có lẽ bạn bè của cô bé sẽ giúp cô bé thoát khỏi tình trạng vô hình.";
"ap24_fq02_chat_01" = "Con cứ ngủ thoải mái. Sáng mai sẽ có trà cho con bất cứ lúc nào con muốn.";
"ap24_fq02_story_01" = "Chiếc chuông bạc leng keng khi Moominmamma lên lầu.";
"ap24_fq02_story_02" = "Giờ thì ngủ ngon nhé, Ninny.";
"ap24_fq03_chat_01" = "Mamma! Cô ấy có bàn chân rồi! Con có thể nhìn thấy bàn chân của cô ấy!";
"ap24_fq03_chat_02" = "Tuyệt! Còn vui hơn nữa khi khuôn mặt cô bé sẽ hiện ra vào một ngày nào đó.";
"ap24_fq03_chat_03" = "Họ hái táo. Đó là một buổi sáng mùa thu đẹp trời.";
"ap24_fq03_chat_04" = "Giỏi lắm, Ninny!";
"ap24_fq03_chat_05" = "Ồ, đó là cái lọ chúng ta dùng để cho ong vò vẽ.";
"ap24_fq03_chat_06" = "Giờ chúng ta không cần mang nó xuống cánh đồng nữa.";
"ap24_fq03_chat_07" = "Con nhìn thấy chân của cô ấy rồi!";
"ap24_fq04_chat_01" = "Cảm ơn tất cả mọi người rất nhiều.";
"ap24_fq04_chat_02" = "Chúng tôi rất vui khi hôm nay được thấy con rõ hơn!";
"ap24_fq04_chat_03" = "Thật tốt vì con đã bắt đầu trò chuyện. Con có biết trò chơi nào hay không?";
"ap24_fq04_chat_04" = "Không…";
"ap24_fq04_chat_05" = "...nhưng con có nghe nói về các trò chơi!";
"ap24_fq04_story_01" = "Tối hôm đó, Moominmamma đã may cho Ninny một chiếc váy màu hồng phấn và đặt chiếc váy trong căn phòng gác mái phía đông.";
"ap24_fq04_story_02" = "Ninny xuống nhà để cùng gia đình Moomin uống trà sáng.";
"ap24_fq05_chat_01" = "Ninny đã hiện ra đến cổ, và chỉ còn lại khuôn mặt cô ấy.";
"ap24_fq05_chat_02" = "Moomintroll rất vui mừng. Cậu quyết định dạy Ninny tất cả các trò chơi mà cậu biết.";
"ap24_fq05_chat_03" = "Leo lên đi, Ninny!";
"ap24_fq05_chat_04" = "Xin lỗi, tôi không thể...";
"ap24_fq05_chat_05" = "Chạy nào, chạy nào, bạn không chạy được sao!";
"ap24_fq05_chat_06" = "Xin lỗi...";
"ap24_fq05_chat_07" = "Bạn cũng không thể nhảy được sao?";
"ap24_fq05_chat_08" = "Rõ ràng là cô ấy chơi chỉ vì lịch sự chứ không phải để vui.";
"ap24_fq05_chat_09" = "Tôi rất xin lỗi...";
"ap24_fq05_chat_10" = "Bạn nghĩ có ai thích như vậy không?";
"ap24_fq05_chat_11" = "Bạn không có chút sức sống nào trong người sao?";
"ap24_fq05_chat_12" = "Bạn không muốn bị đấm vào mũi đấy chứ?";
"ap24_fq05_chat_13" = "Cô ấy không thể chơi...";
"ap24_fq05_chat_14" = "Cô ấy không thể tức giận.";
"ap24_fq05_chat_15" = "Đó là tình trạng của cô ấy.";
"ap24_fq05_chat_16" = "Bạn sẽ không bao giờ có được khuôn mặt của mình nếu bạn không học cách chiến đấu!";
"ap24_fq05_chat_17" = "Vâng, tất nhiên là vậy rồi.";
"ap24_fq06_chat_01" = "Ninny bị sao vậy?";
"ap24_fq06_chat_01b" = "Cô ấy sợ à?";
"ap24_fq06_chat_02" = "Vâng, Ninny nghĩ rằng biển quá rộng lớn.";
"ap24_fq06_chat_03" = "Xin chào.";
"ap24_fq06_chat_04" = "Đứa trẻ vô hình thế nào rồi?";
"ap24_fq06_chat_05" = "Con bé chỉ còn lại phần đầu là chưa xuất hiện.";
"ap24_fq06_chat_06" = "Bạn có thể giúp chúng tôi kéo thuyền không?";
"ap24_fq06_chat_07" = "Trời, trông lạnh quá.";
"ap24_fq06_chat_08" = "Đã nhiều tuần liền không có gì thú vị.";
"ap24_fq06_chat_09" = "Làm tốt lắm!";
"ap24_fq06_chat_10" = "Tôi cũng không thể làm tốt hơn!";
"ap24_fq06_chat_11" = "Tôi thấy cô ấy rồi, thấy cô ấy rồi!";
"ap24_fq06_chat_12" = "Đừng có đẩy cô ấy xuống biển lớn khủng khiếp này!";
"ap24_fq06_story_01" = "Nhiều ngày trôi qua, và Ninny vẫn chưa hiện ra khuôn mặt.";
"ap24_fq06_story_02" = "Một ngày nọ, cả gia đình xuống bãi biển để kéo thuyền lên trước mùa đông.";
"ap24_fq06_story_03" = "Khi họ ra đến biển, Ninny đột nhiên dừng lại.";
"ap24_fq06_story_04" = "Moominpappa nảy ra một ý tưởng tinh quái và bắt đầu lẻn đến gần Moominmamma.";
"ap24_fq06_story_05" = "Ông thực sự không hề muốn đẩy bà ấy xuống nước, ông chỉ muốn làm bọn trẻ vui một chút.";
