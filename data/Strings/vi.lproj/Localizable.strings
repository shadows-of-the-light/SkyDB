"ap26_fetch03_intro_00" = "\"Ta biết về một chú chim, xanh như vải chàm...\"";
"ap26_fetch03_intro_01" = "Đi theo <1>Anh Họ Pháo Hoa Hóa Trang</1> đến Sa Mạc Ánh Sao để tìm <1>Chim Xanh</1> chứ?";
"ap26_fetch03_intro_02" = "... tiếng hót của nó ngân vang nơi nào có đám đông ngưỡng mộ.";
"ap26_fetch03_outro_02" = "... nhưng nó bung ra như sợi chỉ lỏng lẻo.\"";
"ap26_fetch04_dialog_01" = "\"Khu rừng yên bình này là nhà của rất nhiều loài chim sặc sỡ.\"";
"ap26_fetch04_dialog_02" = "\"Tôi chắc chắn chúng ta có thể tìm thấy chim xanh ở đây!\"";
"ap26_fetch04_intro_00" = "\"Tôi biết một loài chim, xanh như dòng sông nguyên sơ...";
"ap26_fetch04_intro_01" = "Đi theo <1>Phụ Huynh Kêu Cầu Đốn Cây</1> đến Cao Nguyên Thanh Vắng để tìm <1>Chim Xanh</1>?";
"ap26_fetch04_intro_02" = "...tiếng hót của nó phản chiếu sự hài hòa yên bình của khu rừng.";
"ap26_fetch04_outro_01" = "\"Một con chim đã được tìm thấy, lông của nó màu xanh lam...";
"ap26_fetch04_outro_02" = "...nhưng nó đã tan tác trong gió và chỉ để lại sự im lặng.\"";
"ap26_fetch05_intro_00" = "\"Tôi biết một loài chim, xanh như những vì sao...";
"ap26_fetch05_intro_01" = "Đi theo <1>Ông Bà Tiên Tri Minh Triết</1> để tìm <1>Chim Xanh</1>?";
"ap26_fetch05_intro_02" = "...tiếng hót của nó vang vọng nơi những định mệnh vĩ đại ra đời.”";
"ap26_fetch05_outro_01" = "\"Một con chim đã được tìm thấy, lông của nó màu xanh lam...";
"ap26_fetch05_outro_02" = "...nhưng giống như định mệnh, nó không chỉ đơn thuần là để nắm giữ.\"";
"ap26_fetch05_quest_01" = "\"Những đứa trẻ này biết định mệnh của mình, và đang chuẩn bị ra đời.\"";
"ap26_fetch05_quest_02" = "“Những người khác cần sự giúp đỡ của chim xanh để nói lên định mệnh của họ.\"";
"ap26_fetch05_quest_03" = "\"Hãy nói cho ta biết định mệnh của con.\"";
"ap26_fetch05_quest_04" = "\"Con sẽ trở thành một người làm vườn và làm cho những điều tươi đẹp lớn lên!\"";
"ap26_fetch05_quest_05" = "\"Khi con ra đời, con sẽ phát minh ra những cỗ máy chữa lành bệnh tật!\"";
"ap26_fetch05_quest_06" = "\"Con sẽ hát những bài ca mang lại hy vọng cho những người đang tuyệt vọng!\"";
"ap26_fetch05_quest_07" = "\"Biết được định mệnh của mình chỉ là sự khởi đầu...\"";
"ap26_fetch06_quest_01" = "Bạn trân trọng điều gì hơn sự giàu sang?";
"ap26_fetch06_quest_02" = "Bạn nhớ điều gì thúc đẩy bạn tiến về phía trước?";
