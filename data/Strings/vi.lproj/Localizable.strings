"ap24_side04_chat_06" = "Ừ.";
"ap24_side04_chat_07" = "Bạn đã mất món đồ chơi…";
"ap24_side04_chat_08" = "...nhưng có lẽ bạn đã nhận được nhiều hơn những gì bạn nghĩ.";
"ap24_side06_chat_01" = "Bí mật của họ liên quan đến những cơn giông bão!";
"ap24_side06_chat_02" = "Chính là những cơn giông mà họ luôn tìm kiếm và mong chờ...";
"ap24_side06_chat_03" = "Có chuyện gì với mình vậy?";
"ap24_side06_chat_04" = "Mình không phải là Hattifattener, mình là Moominpappa...";
"ap24_side06_chat_05" = "Mình đang làm gì ở đây?";
"ap24_side06_chat_06" = "Mình đang về nhà! Mình sẽ rời đi ngay lập tức!";
"ap24_side07_story_01" = "Có một lần, Moominpappa bỏ đi mà không một lời giải thích.";
"ap24_side07_story_02" = "Không ai biết chắc chắn ông ấy đi đâu.";
"ap24_side07_story_03" = "Trong suốt cuộc đời, Moominpappa chưa bao giờ cảm thấy dễ chịu đến vậy.";
