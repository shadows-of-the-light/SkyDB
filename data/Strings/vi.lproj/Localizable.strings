"ap24_side02_chat_14" = "Một năm trôi qua nhanh lắm!";
"ap24_side03_chat_01" = "Bạn là - ờ - một họa sĩ phải không?";
"ap24_side03_chat_02" = "Hay có lẽ là một nhà thơ?";
"ap24_side03_chat_03" = "Tôi là tất cả mọi thứ!";
"ap24_side03_chat_04" = "Và tôi thấy bạn là nhà khám phá.";
"ap24_side03_chat_05" = "Bạn đang nghĩ đến việc khám phá điều gì?";
"ap24_side03_chat_06" = "Ồ, mọi thứ.";
"ap24_side03_chat_07" = "Ví dụ như các vì sao!";
"ap24_side03_chat_08" = "Các vì sao!";
"ap24_side03_chat_09" = "Vậy tôi phải đi cùng bạn.";
"ap24_side03_chat_10" = "Các vì sao là thứ tôi yêu thích nhất.";
"ap24_side03_chat_11" = "Tôi luôn nằm và nhìn ngắm chúng trước khi đi ngủ…";
"ap24_side03_chat_12" = "...và tự hỏi ai đang ở trên đó.";
"ap24_side04_chat_01" = "Haiz...";
"ap24_side04_chat_02" = "Bạn có vẻ buồn.";
"ap24_side04_chat_03" = "Tôi đã tặng đi món đồ chơi yêu thích của mình.";
